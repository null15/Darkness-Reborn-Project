public function s__CollisionDatabase_motion (integer this_ )
	unit a
	group g
	integer c
	s__CollisionDatabase_distance[this_]=s__CollisionDatabase_distance[this_]-s__CollisionDatabase_speed[this_]
	if s__CollisionDatabase_distance[this_]>0. 
		s__CollisionDatabase_x[this_]=s__CollisionDatabase_x[this_]+s__CollisionDatabase_speed[this_]*Cos(s__CollisionDatabase_angle[this_])
		s__CollisionDatabase_y[this_]=s__CollisionDatabase_y[this_]+s__CollisionDatabase_speed[this_]*Sin(s__CollisionDatabase_angle[this_])
		SetUnitPosition(s__CollisionDatabase_u[this_],s__CollisionDatabase_x[this_],s__CollisionDatabase_y[this_])
		SetUnitFlyHeight(s__CollisionDatabase_u[this_],s__CollisionDatabase_z[this_],s__CollisionDatabase_Rz[this_])
		g=GetUnitsInRange(s__CollisionDatabase_range[this_],s__CollisionDatabase_x[this_],s__CollisionDatabase_y[this_])
		c=0
		while true
			a=FirstOfGroup(g)
			if a==null
				break
			GroupRemoveUnit(g,a)
			if IsUnitNotImmun(s__CollisionDatabase_attacker[this_],a)and a!=s__CollisionDatabase_u[this_]
				c=c+1
			
		
		GroupClear(g)
		g=null
		s__CollisionDatabase_active[this_]=c<=0
	else
		s__CollisionDatabase_active[this_]=false
	

