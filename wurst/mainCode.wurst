public function Trig_Repair1_Func004001 () returns boolean
	return(GetUnitStateSwap(UNIT_STATE_LIFE,udg_Repair_Target)==GetUnitStateSwap(UNIT_STATE_MAX_LIFE,udg_Repair_Target))

public function Trig_Repair1_Actions ()
	SetUnitManaBJ(udg_Reair_Cast,(GetUnitStateSwap(UNIT_STATE_MANA,udg_Reair_Cast)-I2R(udg_Repair_Interger)))
	SetUnitLifeBJ(udg_Repair_Target,(GetUnitStateSwap(UNIT_STATE_LIFE,udg_Repair_Target)+I2R(udg_Repair_Interger)))
	if((GetUnitStateSwap(UNIT_STATE_MANA,udg_Reair_Cast)<=I2R(udg_Repair_Interger)))
		DisableTrigger(GetTriggeringTrigger())
	else
		DoNothing()
	
	if((GetUnitStateSwap(UNIT_STATE_LIFE,udg_Repair_Target)==GetUnitStateSwap(UNIT_STATE_MAX_LIFE,udg_Repair_Target)))
		DisableTrigger(GetTriggeringTrigger())
	else
		DoNothing()
	

