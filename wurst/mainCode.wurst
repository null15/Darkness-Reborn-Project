public function StringContainsStringByPos (string source,string search,boolean left,integer pos ) returns integer
	integer maxi=StringLength(source)
	integer strl=StringLength(search)
	integer pos2
	pos2 = pos
	if(strl>maxi)
		return-1
	
	if(left)
		while true
			if(pos+strl>maxi)
				break
			if(SubString(source,pos,pos+strl)==search)
				return(pos)
			
			pos2=pos+1
			if(pos+strl>maxi)
				break
			if(SubString(source,pos,pos+strl)==search)
				return(pos)
			
			pos2=pos+1
		
	else
		while true
			if(pos-strl<0)
				break
			if(SubString(source,pos-strl,pos)==search)
				return(pos-strl)
			
			pos2=pos-1
			if(pos-strl<0)
				break
			if(SubString(source,pos-strl,pos)==search)
				return(pos-strl)
			
			pos2=pos-1
		
	
	return-1

