		i=0
		tempint=0
		tempdivider=100
		while true
			if i==3
				break
			tempint=R2I(armorcalc/ tempdivider)
			SetUnitAbilityLevel(target,armor[i],tempint+1)
			armorcalc=armorcalc-I2R(tempint*tempdivider)
			tempdivider=R2I(tempdivider/ 10)
			i=i+1
		
	

public function MS_DamageTarget ()
	timer t=GetExpiredTimer()
	integer timerID=GetHandleId(t)
	unit caster=LoadUnitHandle(udg_MS_Hashtable,timerID,0)
	unit target=LoadUnitHandle(udg_MS_Hashtable,timerID,1)
	integer id=LoadInteger(udg_MS_Hashtable,timerID,2)
	group g=LoadGroupHandle(udg_MS_Hashtable,id,3)
	real dmg=LoadReal(udg_MS_Hashtable,timerID,3)
	integer unitId=GetHandleId(target)
	integer count=LoadInteger(udg_MS_Hashtable,unitId,id)
	integer i=(GetUnitAbilityLevel((caster),('A0ZA')))
	UnitDamageTarget(caster,target,dmg,false,false,(ATTACK_TYPE_MAGIC),(DAMAGE_TYPE_UNIVERSAL),(WEAPON_TYPE_WHOKNOWS))
	if(true)and IsUnitType(target,UNIT_TYPE_STRUCTURE)
		MS_ModifyArmor(target,((i)))
		count=count+((i))
		SaveInteger(udg_MS_Hashtable,unitId,id,count)
	else
		if not IsUnitType(target,UNIT_TYPE_STRUCTURE)
			MS_ModifyArmor(target,((i)))
			count=count+((i))
			SaveInteger(udg_MS_Hashtable,unitId,id,count)
		
	
	if IsUnitType(target,UNIT_TYPE_DEAD)
		UnitRemoveAbility(target,('A0ZF'))
	
	if g!=null 
		GroupAddUnit(g,target)
	else
		g=CreateGroup()
		GroupAddUnit(g,target)
	
	dmg=dmg*(1.00+MS_DmgPlusRate(i)/ 100)
	SaveGroupHandle(udg_MS_Hashtable,id,3,g)
	SaveReal(udg_MS_Hashtable,id,4,dmg)
	FlushChildHashtable(udg_MS_Hashtable,timerID)
	PauseTimer(t)
	DestroyTimer(t)
	caster=null
	target=null
	g=null
	t=null

public function MS_LifeCheck (unit caster,unit stormfx,integer id,real dmg )
	real lifecheck
	group g=CreateGroup()
	real x=GetUnitX(caster)
	real y=GetUnitY(caster)
	unit target
	timer t
	unit u
	integer timerID
	real r
	GroupEnumUnitsInRange(g,x,y,(400+((GetUnitAbilityLevel((caster),('A0ZA'))))*100.),null)
	while true
		target=FirstOfGroup(g)
		if target==null or MS_FilterUnit(caster,target)and not IsUnitInGroup(target,LoadGroupHandle(udg_MS_Hashtable,id,5))
			break
		GroupRemoveUnit(g,target)
	
	if target!=null 
		lifecheck=GetUnitState(target,UNIT_STATE_LIFE)
		if(true)
			lifecheck=lifecheck/ GetUnitState(target,UNIT_STATE_MAX_LIFE)
		
		while true
			u=FirstOfGroup(g)
			if u==null
				break
			if MS_FilterUnit(caster,u)and not IsUnitInGroup(u,LoadGroupHandle(udg_MS_Hashtable,id,5))
				r=GetUnitState(u,UNIT_STATE_LIFE)
				if(true)
					r=r/ GetUnitState(u,UNIT_STATE_MAX_LIFE)
				
				if r<lifecheck 
					lifecheck=r
					target=u
				
			
			GroupRemoveUnit(g,u)
		
		t=CreateTimer()
		timerID=GetHandleId(t)
		SaveUnitHandle(udg_MS_Hashtable,timerID,0,caster)
		SaveUnitHandle(udg_MS_Hashtable,timerID,1,target)
		SaveInteger(udg_MS_Hashtable,timerID,2,id)
		SaveReal(udg_MS_Hashtable,timerID,3,dmg)
		TimerStart(t,0.3,false,function MS_DamageTarget)
		GroupAddUnit(LoadGroupHandle(udg_MS_Hashtable,id,5),target)
		IssueTargetOrder(stormfx,("fingerofdeath"),target)
		target=null
		t=null
	
	DestroyGroup(g)
	g=null

public function MS_Periodic ()
	trigger trig=GetTriggeringTrigger()
	integer trigID=GetHandleId(trig)
	unit caster=LoadUnitHandle(udg_MS_Hashtable,trigID,1)
	unit stormfx=LoadUnitHandle(udg_MS_Hashtable,trigID,2)
	integer evalcount=GetTriggerEvalCount(trig)
	real x=GetUnitX(caster)
	real y=GetUnitY(caster)
	group g
	real dmg=LoadReal(udg_MS_Hashtable,trigID,4)
	timer t
	integer count
	unit u
	integer unitid
	integer i=0
	if(GetTriggerEventId()==EVENT_UNIT_DEATH)
		if(GetDyingUnit()==caster)
			KillUnit(stormfx)
		
		DestroyGroup(LoadGroupHandle(udg_MS_Hashtable,trigID,5))
		KillUnit(stormfx)
		g=LoadGroupHandle(udg_MS_Hashtable,trigID,3)
		while true
			u=FirstOfGroup(g)
			if u==null
				break
			unitid=GetHandleId(u)
			count=LoadInteger(udg_MS_Hashtable,unitid,trigID)
			MS_ModifyArmor(u,(-count))
			GroupRemoveUnit(g,u)
		
		DestroyGroup(g)
		FlushChildHashtable(udg_MS_Hashtable,trigID)
		g=null
		DisableTrigger(trig)
		TriggerRemoveAction(trig,LoadTriggerActionHandle(udg_MS_Hashtable,trigID,0))
		trig=null
		h__DestroyTrigger(GetTriggeringTrigger())
	else
		GroupClear(LoadGroupHandle(udg_MS_Hashtable,trigID,5))
		if evalcount==1 or evalcount>=LoadInteger(udg_MS_Hashtable,trigID,6)
			while true
				if i==(((GetUnitAbilityLevel((caster),('A0ZA')))))
					break
				MS_LifeCheck(caster,stormfx,trigID,dmg)
				i=i+1
			
			if(false)
				SaveInteger(udg_MS_Hashtable,trigID,6,evalcount+GetRandomInt(R2I((1.00)/ 0.03125),R2I((2.00)/ 0.03125)))
			else
				SaveInteger(udg_MS_Hashtable,trigID,6,evalcount+R2I((1.25-((GetUnitAbilityLevel((caster),('A0ZA'))))*0.15)/ 0.03125))
			
		
		SetUnitPosition(stormfx,x,y)
	
	caster=null
	stormfx=null
	trig=null

