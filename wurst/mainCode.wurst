public function UnitEventInit ()
	integer i = bj_MAX_PLAYER_SLOTS //update to_ make it_ work with 1.29
	player p
	trigger t = CreateTrigger()
	trigger load = CreateTrigger()
	trigger death = CreateTrigger()
	trigger summon = CreateTrigger()
	rect r = GetWorldBounds()
	region re = CreateRegion()
	boolexpr enterB = Filter(function UnitEventEnter)
	boolexpr orderB = Filter(function UnitEventOnOrder)
	udg_WorldMaxX = GetRectMaxX(r)
	udg_WorldMaxY = GetRectMaxY(r)
	RegionAddRect(re, r)
	RemoveRect(r)
	UnitEventDestroyGroup(0)
	UnitEventDestroyGroup(1)
	
	udg_CheckDeathList[0] = 0
	udg_UnitIndexerEnabled = true
	TriggerRegisterEnterRegion(CreateTrigger(), re, enterB)
	TriggerAddCondition(load, Filter(function UnitEventOnLoad))
	TriggerAddCondition(death, Filter(function UnitEventOnDeath))
	TriggerAddCondition(summon, Filter(function UnitEventOnSummon))
	while true
		i = i - 1
		p = Player(i)
		SetPlayerAbilityAvailable(p, udg_DetectRemoveAbility, false)
		SetPlayerAbilityAvailable(p, udg_DetectTransformAbility, false)
		TriggerRegisterPlayerUnitEvent(summon, p, EVENT_PLAYER_UNIT_SUMMON, null)
		TriggerRegisterPlayerUnitEvent(t, p, EVENT_PLAYER_UNIT_ISSUED_ORDER, orderB)
		TriggerRegisterPlayerUnitEvent(death, p, EVENT_PLAYER_UNIT_DEATH, null)
		TriggerRegisterPlayerUnitEvent(load, p, EVENT_PLAYER_UNIT_LOADED, null)
		GroupEnumUnitsOfPlayer(bj_lastCreatedGroup, p, enterB)
		if i == 0
			break
	
	summon = null
	death = null
	load = null
	re = null
	enterB = null
	orderB = null
	p = null
	r = null
	t = null

