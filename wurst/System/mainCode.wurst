public function RK_Fire_Mage_Get_Mult (unit u ) returns real
	return 0.1 * (1 + GetUnitAbilityLevel(u, 'A004') + 4 * GetUnitAbilityLevel(u, 'A0MN'))


public function RK_DDS_Fire_Mage ()
	unit src = GetEventDamageSource()
	unit target = GetTriggerUnit()
	real dmg = GetEventDamage()
	real dmg2 = 0
	real abilLvl = 0
	
	if (dmg <= 0) 
		return
	
	// fire mage fire shield return damage
	if (GetUnitAbilityLevel(target, 'A0E7') > 0) 
		abilLvl = RK_Fire_Mage_Get_Mult(target)
		dmg2 = GetHeroInt(target, true) * (0.25 + abilLvl)
		RK_DDS_Deal_Damage_Generic(target, src, dmg2)
		
		CreateTextTagUnitBJ("|cff0042ff"+I2S(R2I(dmg2))+"|r",src,0,10,100.00,100.00,100,0)
		SetTextTagVisibility(GetLastCreatedTextTag(), true)
		// if (GetOwningPlayer(target) != GetLocalPlayer()) 
		//     SetTextTagVisibility(GetLastCreatedTextTag(), false)
		// 
		SetTextTagVelocityBJ(GetLastCreatedTextTag(),72,90)
		SetTextTagFadepointBJ(GetLastCreatedTextTag(),2.00)
		SetTextTagLifespanBJ(GetLastCreatedTextTag(),1.00)
		SetTextTagPermanentBJ(GetLastCreatedTextTag(),false)
	
	if (BlzGetEventIsAttack()) 
		return
	
	// blood mage bonus int scaling
	if (GetUnitAbilityLevel(src, 'A901') > 0) 
		abilLvl = RK_Fire_Mage_Get_Mult(src)
		dmg2 = dmg + (GetHeroInt(src, true) * abilLvl)
		BlzSetEventDamage(dmg2)
		CreateTextTagUnitBJ("|cff0042ff"+I2S(R2I(dmg2))+"|r",target,0,10,100.00,100.00,100,0)
		SetTextTagVisibility(GetLastCreatedTextTag(), true)
		// if (GetOwningPlayer(src) != GetLocalPlayer()) 
		//     SetTextTagVisibility(GetLastCreatedTextTag(), false)
		// 
		SetTextTagVelocityBJ(GetLastCreatedTextTag(),72,90)
		SetTextTagFadepointBJ(GetLastCreatedTextTag(),2.00)
		SetTextTagLifespanBJ(GetLastCreatedTextTag(),1.00)
		SetTextTagPermanentBJ(GetLastCreatedTextTag(),false)
	
public function RK_DDS_Shadow_Hunter ()
	unit src = GetEventDamageSource()
	unit target = GetTriggerUnit()
	real dmg = GetEventDamage()
	real dmg2 = 0
	real lvl = 0
	
	if dmg <= 0 
		return
	
	if (GetUnitTypeId(src) != 'H123' and GetUnitTypeId(src) != 'H123') 
		return
	
	lvl = I2R(1 + GetUnitAbilityLevel(src, 'A019'))
	if (GetUnitAbilityLevel(target, 'B901') > 0) and (lvl > 1) 
		dmg2 = dmg * lvl
		BlzSetEventDamage(dmg2)
		
		CreateTextTagUnitBJ("|cff0042ff"+I2S(R2I(dmg2))+"! (x" + I2S(R2I(lvl)) + ")|r",target,0,10,100.00,100.00,100,0)
		SetTextTagVisibility(GetLastCreatedTextTag(), true)
		// if (GetOwningPlayer(src) != GetLocalPlayer()) 
		//     SetTextTagVisibility(GetLastCreatedTextTag(), false)
		// 
		SetTextTagVelocityBJ(GetLastCreatedTextTag(),72,90)
		SetTextTagFadepointBJ(GetLastCreatedTextTag(),2.00)
		SetTextTagLifespanBJ(GetLastCreatedTextTag(),1.00)
		SetTextTagPermanentBJ(GetLastCreatedTextTag(),false)
	
