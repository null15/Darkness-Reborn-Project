public function DarkTorture___MoveEnumUnits ()
	integer s=(GetUnitUserData(GetEnumUnit()))
	integer tx=(s__s__DarkTorture___triggerx_TX_Ptr[ArrayId(GetTriggeringTrigger())])
	sound snd
	real d
	
	if(s__DarkTorture___sword_EffectDist[s]!=0.)
		s__DarkTorture___sword_MoveEffects(s)
		if(s__DarkTorture___sword_EffectDist[s]==0.)
			s__DarkTorture___sword_DestroyEffects(s)
		
	
	if(s__DarkTorture___sword_CanMove[s])
		s__DarkTorture___sword_MoveSword(s)
		if(s__DarkTorture___sword_Victim[s]==null)
			s__DarkTorture___sword_Victim[s]=DarkTorture___GetNearestUnit(s__DarkTorture___sword_posX[s],s__DarkTorture___sword_posY[s],DarkTorture___SWORD_COLLISION,s__DarkTorture___sword_id[s])
			if(s__DarkTorture___sword_Victim[s]!=null)
				d=rmin(DarkTorture___SingleSwordDamage[s__DarkTorture___sword_Level[s]],s__DarkTorture___triggerx_dmg[tx])
				if(d>0.)
					UnitDamageTarget(s__DarkTorture___sword_Sword[s],s__DarkTorture___sword_Victim[s],d,true,false,ATTACK_TYPE_CHAOS,DAMAGE_TYPE_DEATH,WEAPON_TYPE_WHOKNOWS)
