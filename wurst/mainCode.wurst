	if(10.+(2.*(GetUnitAbilityLevel(a,Crush___SpellId))))>=rndm and Crush___UnitFilter(a,u)
		TriggerAddDamageEvent(u,a,function Crush___CrushBegin)
	
	a=null
	u=null

public function Crush___init ()
	integer i=0
	unit upre
	gg_trg_Crush=CreateTrigger()
	Crush___MaxArea=bj_mapInitialPlayableArea
	while true
		TriggerRegisterPlayerUnitEvent(gg_trg_Crush,Player(i),EVENT_PLAYER_UNIT_ATTACKED,MainFunctions_filter)
