public function Sleep (real seconds ) returns real
	real timRem=0.
	real sleep=0.
	real timRemMain=0.
	integer n=0
	boolean timFinish=FALSE
	if(seconds<=0. and seconds!=(-1.00))
		return 0
	
	if(seconds<=bj_WAIT_FOR_COND_MIN_INTERVAL)
		TriggerSleepAction(-1)
		return 0
	
	timRemMain=TimerGetRemaining(NativeDDUniversalPack___sleepTimer)-seconds
	timRem=seconds
	if(timRemMain<0.)
		TriggerSleepAction(seconds)
		return 0
	
	if(seconds-.5>0.)
		sleep=seconds-.5
		while true
			if(timRem<=.5)
				break

			TriggerSleepAction(sleep)
			timRem=TimerGetRemaining(NativeDDUniversalPack___sleepTimer)-timRemMain
			if(timRem>.5)
				sleep=timRem-.5
			
		
	else
		timRem=seconds
	
	sleep=timRem*bj_WAIT_FOR_COND_MIN_INTERVAL
	n=R2I(timRem/ bj_WAIT_FOR_COND_MIN_INTERVAL)
	if(R2I(timRem/ bj_WAIT_FOR_COND_MIN_INTERVAL)==n)
		n=n-1
	
	while true
		timFinish=((timRem<=0.026)and(n<=0))or(timRem<=sleep)
		if(timFinish)
			break
		
		TriggerSleepAction(-1)
		n=n-1
		timRem=TimerGetRemaining(NativeDDUniversalPack___sleepTimer)-timRemMain

	return 0

