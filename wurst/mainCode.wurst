	if(s__NativeDDUniversalPack___pGrp[grp]!=0 and whichUnit!=null)
		s__NativeDDUniversalPack___AttUnit[s__NativeDDUniversalPack___pGrp[grp]-1]=whichUnit
		s__dd_vector_Move(s__NativeDDUniversalPack___AttVec[s__NativeDDUniversalPack___pGrp[grp]-1],ox,oy,oz)
