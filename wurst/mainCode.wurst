	g=(s__graphic_create((TerrorBlade___TERROR_EFFECT),((x2)*1.0),((y2)*1.0),((TerrorBlade___TERROR_Z)*1.0),((rad*RADTODEG)*1.0),((TerrorBlade___TERROR_SIZE*0.01)*1.0),90.))
