				DestroyEffect(AddSpecialEffectTarget(DarkTorture___BLOOD_EFFECT,s__DarkTorture___sword_Victim[s],DarkTorture___ATTACH_POINT))
				s__DarkTorture___sword_Speed[s]=rmax(s__DarkTorture___sword_Speed[s]-DarkTorture___ON_ADD_SPEED_DECREASE,DarkTorture___ON_ADD_SPEED_DECREASE)
				SetUnitTimeScale(s__DarkTorture___sword_Victim[s],0.1)
				SetUnitAnimation(s__DarkTorture___sword_Victim[s],"death")
			
		else
			s__DarkTorture___sword_PickUnits(s)
		
	
	
	DarkTorture___TempSword=(s)
	
	if(not IsTerrainPathable(s__DarkTorture___sword_posX[s]+DarkTorture___SWORD_COLLISION*s__DarkTorture___sword_x[s],s__DarkTorture___sword_posY[s]+DarkTorture___SWORD_COLLISION*s__DarkTorture___sword_y[s],PATHING_TYPE_WALKABILITY))
		ForGroup(s__DarkTorture___sword_PushGroup[s],function DarkTorture___MoveEnumSwordUnits)
	else
		s__DarkTorture___sword_PushRange[s]=0.
		s__DarkTorture___sword_CanMove[s]=false
		ForGroup(s__DarkTorture___sword_PushGroup[s],function DarkTorture___MoveEnumSwordUnits)
	
	
	if(s__DarkTorture___sword_PushRange[s]<=0.)
		ForGroup(s__DarkTorture___sword_PushGroup[s],function DarkTorture___MoveEnumSwordUnits)
		bj_lastCreatedUnit=CreateUnit(s__DD_p[s__DarkTorture___sword_id[s]],DarkTorture___DUMMY_RAWCODE,GetUnitX(s__DarkTorture___sword_Victim[s]),GetUnitY(s__DarkTorture___sword_Victim[s]),0.)
		UnitAddAbility(bj_lastCreatedUnit,DarkTorture___SLOW_RAWCODE)
		UnitAddAbility(bj_lastCreatedUnit,DarkTorture___LOCUST_RAWCODE)
		UnitApplyTimedLife(bj_lastCreatedUnit,'BTLF',0.25)
		IssueTargetOrder(bj_lastCreatedUnit,"slow",s__DarkTorture___sword_Victim[s])
		s__DarkTorture___sword_deallocate(s)
		GroupRemoveUnit(s___DarkTorture___triggerx_g[s__DarkTorture___triggerx_g[tx]],GetEnumUnit())
		return
	
	
	if(s__DarkTorture___sword_Distance[s]<=0.)
		s__DarkTorture___sword_PushRange[s]=0.
		ForGroup(s__DarkTorture___sword_PushGroup[s],function DarkTorture___MoveEnumSwordUnits)
		GroupRemoveUnit(s___DarkTorture___triggerx_g[s__DarkTorture___triggerx_g[tx]],s__DarkTorture___sword_Sword[s])
		bj_lastCreatedUnit=CreateUnit(s__DD_p[s__DarkTorture___sword_id[s]],DarkTorture___DUMMY_RAWCODE,GetUnitX(s__DarkTorture___sword_Victim[s]),GetUnitY(s__DarkTorture___sword_Victim[s]),0.)
		UnitAddAbility(bj_lastCreatedUnit,DarkTorture___SLOW_RAWCODE)
		UnitAddAbility(bj_lastCreatedUnit,DarkTorture___LOCUST_RAWCODE)
		UnitApplyTimedLife(bj_lastCreatedUnit,'BTLF',0.25)
