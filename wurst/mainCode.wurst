public function ElectricField__ELFieldAction ()
	unit u=GetTriggerUnit()
	location l=GetSpellTargetLoc()
	real x=GetLocationX(l)
	real y=GetLocationY(l)
	integer i=ElectricField__INT_NULL
	sound s=null
	real cx=ElectricField__REAL_NULL
	real cy=ElectricField__REAL_NULL
	real d=ElectricField__REAL_NULL
	group g=null
	integer lvl=GetUnitAbilityLevel(u,ElectricField__ABILITY_ELECTRIC_FIELD)
	boolexpr bx=null
	integer t
	integer bl
	if(lvl>ElectricField__Levels)
		BJDebugMsg("|cffff0000Unable to_ cast spell: Electric Field, current spell level is greater then defined.|r")
		return
	
	s=CreateSound(ElectricField__SOUND_THUNDER,false,false,false,10,10,"")
	while true
		if(i>=bj_MAX_PLAYERS)
			break
		if(GetLocalPlayer()==s__DD_p[i])
			cx=GetCameraTargetPositionX()
			cy=GetCameraTargetPositionY()
			d=SquareRoot(hypot2(cx-x,cy-y))
			if(d>ElectricField__SOUND_MAX_DISTANCE)
				d=ElectricField__SOUND_MAX_DISTANCE
			
			SetSoundVolume(s,R2I(ElectricField__SOUND_THUNDER_VOL-d*(ElectricField__SOUND_THUNDER_VOL/ ElectricField__SOUND_MAX_DISTANCE)))
			if(d<=ElectricField__CAMERA_NOISE_RANGE and ElectricField__USE_CAMERA_NOISE)
				CameraSetSourceNoise(70.-(d*(70./ ElectricField__CAMERA_NOISE_RANGE)),1750.-(d*(1750./ ElectricField__CAMERA_NOISE_RANGE)))
			
		
		i=i+1
	
	StartSound(s)
	KillSoundWhenDone(s)
	s=null
	DestroyEffect(AddSpecialEffect(ElectricField__GREAT_LIGHTNING_QModEL,x,y))
	if(ElectricField__NUMBER_OF_BOLTS>ElectricField__INT_NULL)
		bl=s__ElectricField__bolts_create(function ElectricField__LoadThunderbolts,x,y,lvl)
		TriggerRegisterTimerEvent(s__ElectricField__bolts_t[bl],ElectricField__SINGLE_BOLT_DELAY,TRUE)
	
	if(ElectricField__THUNDER_DELAY>ElectricField__REAL_NULL)
		Sleep(ElectricField__THUNDER_DELAY)
	
	g=CreateGroup()
	bx=Filter(function ElectricField__ELFieldGroupFilter)
	GroupEnumUnitsInRange(g,x,y,ElectricField__EF_AOE[lvl],bx)
	DestroyBoolExpr(bx)
	ForGroup(g,function ElectricField__EFGroupAction)
	t=s__ElectricField__timerx_create(lvl,g,u)
	TimerStart(s__ElectricField__timerx_t[t],ElectricField__LIGHTNING_EFFECT_DURATION,false,function ElectricField__EFBlowGround)
	Sleep(2.00)
	if(ElectricField__TGCount<=1 and ElectricField__USE_CAMERA_NOISE)
		i=0
		while true
			if(i>=bj_MAX_PLAYERS)
				break
			if(GetLocalPlayer()==s__DD_p[i])
				CameraSetSourceNoise(ElectricField__REAL_NULL,ElectricField__REAL_NULL)
			
			i=i+1
		
	
	RemoveLocation(l)
	l=null
	u=null
	g=null
	bx=null

