public function RK_Troll_Setup ()
	integer idx = 0
	RK_Troll_Names[idx] = "HyoudouIssei"
	idx = idx + 1
	RK_Troll_Names[idx] = RK_Troll_Name_End


public function RK_Is_Troll (string name ) returns boolean
	integer idx = 0
	while true
		if RK_Troll_Names[idx] == RK_Troll_Name_End
			break
		if (name == RK_Troll_Names[idx]) 
			return true
		
		idx = idx + 1
	
	return false


public function RK_Is_Unit_Generic_Damageable (unit u, player p ) returns boolean
	return GetBooleanAnd((IsUnitType(u,UNIT_TYPE_STRUCTURE)==false),(GetBooleanAnd((IsUnitType(u,UNIT_TYPE_MAGIC_IMMUNE)==false),(GetBooleanAnd((IsUnitAliveBJ(u)==true),(IsUnitEnemy(u,p)==true))))))

public function RK_Is_Unit_Damageable_Include_Magic_Immune (unit u, player p ) returns boolean
	return (IsUnitType(u,UNIT_TYPE_STRUCTURE)==false and IsUnitAliveBJ(u)==true and IsUnitEnemy(u,p)==true)

public function CreateTextTagUnit (unit whichUnit,string text,real size,real timeout,integer clr ) returns texttag
	bj_lastCreatedTextTag=CreateTextTag()
	SetTextTagText(bj_lastCreatedTextTag,text,size*0.0023)
	SetTextTagPosUnit(bj_lastCreatedTextTag,whichUnit,0.)
	SetTextTagPermanent(bj_lastCreatedTextTag,false)
	SetTextTagFadepoint(bj_lastCreatedTextTag,timeout*0.66667)
	SetTextTagLifespan(bj_lastCreatedTextTag,timeout)
	SetTextTagVelocity(bj_lastCreatedTextTag,0.,0.0355)
	return(bj_lastCreatedTextTag)

