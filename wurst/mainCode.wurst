					if(Trig_SEARLOOP_Func002Func003Func002Func007Func009C())
						CreateNUnitsAtLoc(1,'n009',GetOwningPlayer(udg_B_SEAR_TRIGGER_UNIT[udg_B_SEAR_CURRENT_INDEX]),GetUnitLoc(udg_B_SEAR_PICKED_UNIT),bj_UNIT_FACING)
