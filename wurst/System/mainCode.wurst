					UnitApplyTimedLifeBJ(1.00,'BTLF',GetLastCreatedUnit())
					UnitDamageTargetBJ(udg_B_SEAR_TRIGGER_UNIT[udg_B_SEAR_CURRENT_INDEX],udg_B_SEAR_PICKED_UNIT,udg_B_SEAR_DAMAGE[udg_B_SEAR_CURRENT_INDEX],ATTACK_TYPE_CHAOS,DAMAGE_TYPE_UNIVERSAL)
