		i=i+1
	

public function DarkTorture___GetNearestUnit (real x,real y,real radius,integer id ) returns unit
	unit u
	unit r
	real md
	real d
	real ux
	real uy
	DarkTorture___TempId=id
	GroupClear(DarkTorture___TempGroup)
	GroupEnumUnitsInRange(DarkTorture___TempGroup,x,y,radius,DarkTorture___GroupFilter)
	md=-1.
	r=null
	while true
		u=FirstOfGroup(DarkTorture___TempGroup)
		if(u==null)
			break
		ux=GetUnitX(u)
		uy=GetUnitY(u)
		d=SquareRoot((ux-x)*(ux-x)+(uy-y)*(uy-y))
		if(md==-1.)
			md=d
			r=u
		
		if(md>d)
			md=d
			r=u
		
		GroupRemoveUnit(DarkTorture___TempGroup,u)
	
	return(r)

