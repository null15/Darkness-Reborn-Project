	udg_Cleaverduration=30
	udg_Cyan=GetTriggerUnit()
	udg_Cyan_loc=GetUnitLoc(udg_Cyan)
	SetUnitInvulnerable(udg_Cyan,true)
	SetUnitVertexColorBJ(udg_Cyan,100,100,100,50.00)
