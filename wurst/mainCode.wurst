public function SetUnitStateEx (unit whichUnit,unitstate whichState,integer value )
	integer abil=NativeDDUniversalPack___DD_LIFE_EDIT_CODE
	integer slvl=2
	integer lvl
	integer value2 = 0
	integer offset
	integer ln
	integer state=R2I(GetUnitState(whichUnit,whichState))
	if(whichState==UNIT_STATE_LIFE or whichState==UNIT_STATE_MANA)
		SetUnitState(whichUnit,whichState,((value)*1.))
		return
	else if(state==value)
		return
	
	if(whichState==UNIT_STATE_MAX_MANA)
		abil=NativeDDUniversalPack___DD_MANA_EDIT_CODE
	
	if(state>value)
		slvl=12
		state=state+value
		value2=state-value
		state=state-value2
	
	ln=imin(log(2,value2-state),9)
	lvl=slvl+ln
	offset=ipow(2,ln)
	while true
		UnitAddAbility(whichUnit,abil)
		SetUnitAbilityLevel(whichUnit,abil,lvl)
		UnitRemoveAbility(whichUnit,abil)
		state=state+offset
		if(state==value)
			if(true)
				break
		
		if(value2-state<offset)
			ln=log(2,value2-state)
			lvl=slvl+ln
			offset=ipow(2,ln)
		
		UnitAddAbility(whichUnit,abil)
		SetUnitAbilityLevel(whichUnit,abil,lvl)
		UnitRemoveAbility(whichUnit,abil)
		state=state+offset
		if(state==value2)
			if(true)
				break
		
		if(value2-state<offset)
			ln=log(2,value2-state)
			lvl=slvl+ln
			offset=ipow(2,ln)
		
		UnitAddAbility(whichUnit,abil)
		SetUnitAbilityLevel(whichUnit,abil,lvl)
		UnitRemoveAbility(whichUnit,abil)
		state=state+offset
		if(state==value2)
			if(true)
				break
		
		if(value2-state<offset)
			ln=log(2,value2-state)
			lvl=slvl+ln
			offset=ipow(2,ln)
		
		UnitAddAbility(whichUnit,abil)
		SetUnitAbilityLevel(whichUnit,abil,lvl)
		UnitRemoveAbility(whichUnit,abil)
		state=state+offset
		if(state==value2)
			if(true)
				break
		
		if(value2-state<offset)
			ln=log(2,value2-state)
			lvl=slvl+ln
			offset=ipow(2,ln)
		
	

