public function InitTrig_RK_Clean_Items ()
	integer i = 0
	gg_trg_Clean_Items=CreateTrigger()
	gg_trg_Clean_Stop=CreateTrigger()
	gg_trg_Clean_Pickup=CreateTrigger()
	while true
		if i >= bj_MAX_PLAYERS
			break
		TriggerRegisterPlayerChatEvent(gg_trg_Clean_Items, Player(i), "-clean", true)
		TriggerRegisterPlayerChatEvent(gg_trg_Clean_Stop, Player(i), "-stopclean", true)
		TriggerRegisterPlayerUnitEvent(gg_trg_Clean_Pickup, Player(i), EVENT_PLAYER_UNIT_PICKUP_ITEM, null)
		i = i+1
	
	TriggerAddAction(gg_trg_Clean_Items,function RK_Clean_Items_Timer_Start)
	TriggerAddAction(gg_trg_Clean_Stop,function RK_Clean_Items_Timer_Stop)
	TriggerAddAction(gg_trg_Clean_Pickup,function RK_Clean_Pickup_Action)

function removeTaverns()
	doAfter(0.1) -> 
		let removeTavern = CreateTimer()
		let removeTavernDialog = CreateTimerDialog(removeTavern)

		removeTavern.start(300, null)
		removeTavernDialog.setTitle("Tavern removal: ")
		removeTavernDialog.setTitleColor(255, 0, 0, 0)
		removeTavernDialog.display(true)

		doAfter(15) -> 
			udg_A_Damage[97]=0
			udg_A_Group[16]=GetUnitsInRectAll(gg_rct_Region_taverns)
			let u = CreateUnitAtLoc(Player(PLAYER_NEUTRAL_AGGRESSIVE),'U236',GetRectCenter(gg_rct_Region_seaelementalspawn),bj_UNIT_FACING)
			u.setLevel(99, false)
				
		doAfter(300) -> 
			removeTavern.destr()
			removeTavernDialog.destr()

			forUnitsInRange(intTavern.getPos(), 2000) (unit u) ->
				RemoveUnit(u)

