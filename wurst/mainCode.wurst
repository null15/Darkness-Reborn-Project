public function ParalyzingField__init ()
	boolexpr b=Condition(function ParalyzingField__Paralyzing_Field_Conditions)
	integer i=0
	unit upre
	player p
	ParalyzingField__frozenall=GetNextGroup()
	gg_trg_Paralyzing_Field=CreateTrigger()
	TriggerAddCondition(gg_trg_Paralyzing_Field,b)
	TriggerAddAction(gg_trg_Paralyzing_Field,function ParalyzingField__Paralyzing_Field_Actions)
	ParalyzingField__endcast=CreateTrigger()
	TriggerAddCondition(ParalyzingField__endcast,b)
	TriggerAddAction(ParalyzingField__endcast,function ParalyzingField__EndParalyzingField)
	while true
		p=Player(i)
		TriggerRegisterPlayerUnitEvent(gg_trg_Paralyzing_Field,p,EVENT_PLAYER_UNIT_SPELL_EFFECT,MainFunctions_filter)
		TriggerRegisterPlayerUnitEvent(ParalyzingField__endcast,p,EVENT_PLAYER_UNIT_SPELL_ENDCAST,MainFunctions_filter)
		i=i+1
		if i==bj_MAX_PLAYER_SLOTS
			break
	
	p=null
	upre=CreateUnit(Player(14),ParalyzingField__DummyId,PreX,PreY,0)
	UnitAddAbility(upre,ParalyzingField__EffectId)
	RemoveUnit(upre)
	upre=null
	Preload(ParalyzingField__FieldMdl)
	b=null

