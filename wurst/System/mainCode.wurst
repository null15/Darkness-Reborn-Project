public function MS_ModifyArmor (unit target,integer armorreduction )
	integer array armor
	integer level1=(GetUnitAbilityLevel((target),('A0ZB')))
	integer level2=(GetUnitAbilityLevel((target),('A0ZD')))
	integer level3=(GetUnitAbilityLevel((target),('A0ZC')))
	real armorreduce=I2R(level1+level2*10+level3*100)
	real armorcalc
	integer tempdivider
	integer tempint
	integer i
	armor[0]=('A0ZC')
	armor[1]=('A0ZD')
	armor[2]=('A0ZB')
	if armorreduce==0 
		UnitAddAbility(target,('A0ZF'))
		armorcalc=armorreduce+armorreduction
		if armorcalc==0 
			UnitRemoveAbility(target,('A0ZF'))
		
		i=0
		tempint=0
		tempdivider=100
		while true
			if i==3
				break
			tempint=R2I(armorcalc/ tempdivider)
			SetUnitAbilityLevel(target,armor[i],tempint+1)
			armorcalc=armorcalc-I2R(tempint*tempdivider)
			tempdivider=R2I(tempdivider/ 10)
			i=i+1
		
	else
		armorreduce=I2R(level1+level2*10+level3*100-111)
		armorcalc=armorreduce+armorreduction
		if armorcalc==0 
			UnitRemoveAbility(target,('A0ZF'))
		
		i=0
		tempint=0
		tempdivider=100
		while true
			if i==3
				break
			tempint=R2I(armorcalc/ tempdivider)
			SetUnitAbilityLevel(target,armor[i],tempint+1)
			armorcalc=armorcalc-I2R(tempint*tempdivider)
			tempdivider=R2I(tempdivider/ 10)
			i=i+1
		
	

public function MS_DamageTarget ()
	timer t=GetExpiredTimer()
	integer timerID=GetHandleId(t)
	unit caster=LoadUnitHandle(udg_MS_Hashtable,timerID,0)
	unit target=LoadUnitHandle(udg_MS_Hashtable,timerID,1)
	integer id=LoadInteger(udg_MS_Hashtable,timerID,2)
	group g=LoadGroupHandle(udg_MS_Hashtable,id,3)
	real dmg=LoadReal(udg_MS_Hashtable,timerID,3)
	integer unitId=GetHandleId(target)
	integer count=LoadInteger(udg_MS_Hashtable,unitId,id)
	integer i=(GetUnitAbilityLevel((caster),('A0ZA')))
	UnitDamageTarget(caster,target,dmg,false,false,(ATTACK_TYPE_MAGIC),(DAMAGE_TYPE_UNIVERSAL),(WEAPON_TYPE_WHOKNOWS))
	if(true)and IsUnitType(target,UNIT_TYPE_STRUCTURE)
		MS_ModifyArmor(target,((i)))
		count=count+((i))
		SaveInteger(udg_MS_Hashtable,unitId,id,count)
	else
		if not IsUnitType(target,UNIT_TYPE_STRUCTURE)
			MS_ModifyArmor(target,((i)))
			count=count+((i))
			SaveInteger(udg_MS_Hashtable,unitId,id,count)
		
	
	if IsUnitType(target,UNIT_TYPE_DEAD)
		UnitRemoveAbility(target,('A0ZF'))
	
	if g!=null 
		GroupAddUnit(g,target)
	else
		g=CreateGroup()
		GroupAddUnit(g,target)
	
	dmg=dmg*(1.00+MS_DmgPlusRate(i)/ 100)
	SaveGroupHandle(udg_MS_Hashtable,id,3,g)
	SaveReal(udg_MS_Hashtable,id,4,dmg)
	FlushChildHashtable(udg_MS_Hashtable,timerID)
	PauseTimer(t)
	DestroyTimer(t)
	caster=null
	target=null
	g=null
	t=null

