public function UnitEventDestroyGroup (integer i )
	if udg_CargoTransportGroup[i] != null 
		DestroyGroup(udg_CargoTransportGroup[i])
		udg_CargoTransportGroup[i] = null
	

public function UnitEventCheckAfter ()
	integer i = 0
	while true
		i = udg_CheckDeathList[i]
		if i == 0
			break
		udg_UDex = i
		if udg_IsUnitNew[i] 
			//The unit was just created.
			udg_IsUnitNew[i] = false
			udg_UnitIndexEvent = 1.50 //New event requested by SpellBound to_ detect when unit fully enters scope.
			udg_UnitIndexEvent = 0.00
		else if udg_IsUnitTransforming[i] 
			//Added 21 July 2017 to_ fix the issue re-adding this_ ability in_ the same instant
			udg_UnitTypeEvent = 0.00
			udg_UnitTypeEvent = 1.00
			udg_UnitTypeOf[i] = GetUnitTypeId(udg_UDexUnits[i]) //Set this_ afterward to_ give the user extra reference
			udg_IsUnitTransforming[i] = false
			UnitAddAbility(udg_UDexUnits[i], udg_DetectTransformAbility)
		else if udg_IsUnitAlive[i] 
			//The unit has started reincarnating.
			udg_IsUnitReincarnating[i] = true
			udg_IsUnitAlive[i] = false
			udg_DeathEvent = 0.50
			udg_DeathEvent = 0.00
		
		udg_CheckDeathInList[i] = false
	
	//Empty the list
	udg_CheckDeathList[0] = 0

public function UnitEventCheckAfterProxy (integer i )
	if udg_CheckDeathList[0] == 0 
		TimerStart(udg_CheckDeathTimer, 0.00, false, function UnitEventCheckAfter)
	
	if not udg_CheckDeathInList[i] 
		udg_CheckDeathList[i] = udg_CheckDeathList[0]
		udg_CheckDeathList[0] = i
		udg_CheckDeathInList[i] = true
	


public function UnitEventOnUnload ()
	integer i = udg_UDex
	GroupRemoveUnit(udg_CargoTransportGroup[GetUnitUserData(udg_CargoTransportUnit[i])], udg_UDexUnits[i])
	udg_IsUnitBeingUnloaded[i] = true
	udg_CargoEvent = 0.00
	udg_CargoEvent = 2.00
	udg_CargoEvent = 0.00
	udg_IsUnitBeingUnloaded[i] = false
	if not IsUnitLoaded(udg_UDexUnits[i]) or IsUnitType(udg_CargoTransportUnit[i], UNIT_TYPE_DEAD) or GetUnitTypeId(udg_CargoTransportUnit[i]) == 0 
		udg_CargoTransportUnit[i] = null
	


public function UnitEventOnDeath () returns boolean
	integer pdex = udg_UDex
	udg_UDex = GetUnitUserData(GetTriggerUnit())
	if udg_UDex != 0 
		udg_KillerOfUnit[udg_UDex] = GetKillingUnit() //Added 29 May 2017 for_ GIMLI_2
		udg_IsUnitAlive[udg_UDex] = false
		udg_DeathEvent = 0.00
		udg_DeathEvent = 1.00
		udg_DeathEvent = 0.00
		udg_KillerOfUnit[udg_UDex] = null
		if udg_CargoTransportUnit[udg_UDex] != null 
			UnitEventOnUnload()
		
	
	udg_UDex = pdex
	return false


public function UnitEventOnOrder () returns boolean
	integer pdex = udg_UDex
	unit u = GetFilterUnit()
	integer i = GetUnitUserData(u)
	if i > 0 
		udg_UDex = i
		if GetUnitAbilityLevel(u, udg_DetectRemoveAbility) == 0 
			if not udg_IsUnitRemoved[i] 
				udg_IsUnitRemoved[i] = true
				udg_IsUnitAlive[i] = false
				udg_SummonerOfUnit[i] = null
				
				//For backwards-compatibility:
				udg_DeathEvent = 0.00
				udg_DeathEvent = 3.00
				udg_DeathEvent = 0.00
				
				//Fire deindex event for_ UDex:
				udg_UnitIndexEvent = 0.00
				udg_UnitIndexEvent = 2.00
				udg_UnitIndexEvent = 0.00
				
				udg_UDexNext[udg_UDexPrev[i]] = udg_UDexNext[i]
				udg_UDexPrev[udg_UDexNext[i]] = udg_UDexPrev[i]
				
				// Recycle the index for_ later use
				udg_UDexUnits[i] = null
				udg_UDexPrev[i] = udg_UDexLastRecycled
				udg_UDexLastRecycled = i
				UnitEventDestroyGroup(i)
			
		else if not udg_IsUnitAlive[i] 
			if not IsUnitType(u, UNIT_TYPE_DEAD) 
				udg_IsUnitAlive[i] = true
				udg_DeathEvent = 0.00
				udg_DeathEvent = 2.00
				udg_DeathEvent = 0.00
				udg_IsUnitReincarnating[i] = false
			
		else if IsUnitType(u, UNIT_TYPE_DEAD) 
			if udg_IsUnitNew[i] 
				//This unit was created as a corpse.
				udg_IsUnitAlive[i] = false
				udg_DeathEvent = 0.00
				udg_DeathEvent = 1.00
				udg_DeathEvent = 0.00
			else if udg_CargoTransportUnit[i] == null or not IsUnitType(u, UNIT_TYPE_HERO) 
				//The unit may have just started reincarnating.
				UnitEventCheckAfterProxy(i)
			
		else if GetUnitAbilityLevel(u, udg_DetectTransformAbility) == 0 and not udg_IsUnitTransforming[i] 
			udg_IsUnitTransforming[i] = true
			UnitEventCheckAfterProxy(i)  //This block has been updated on 21 July 2017
		
		if udg_CargoTransportUnit[i] != null and not udg_IsUnitBeingUnloaded[i] and not IsUnitLoaded(u) or IsUnitType(u, UNIT_TYPE_DEAD) 
			UnitEventOnUnload()
		
		udg_UDex = pdex
	
	u = null
	return false

public function UnitEventOnSummon () returns boolean
	integer pdex = udg_UDex
	udg_UDex = GetUnitUserData(GetTriggerUnit())
	if udg_IsUnitNew[udg_UDex] 
		udg_SummonerOfUnit[udg_UDex] = GetSummoningUnit()
		udg_UnitIndexEvent = 0.00
		udg_UnitIndexEvent = 0.50
		udg_UnitIndexEvent = 0.00
	
	udg_UDex = pdex
	return false

public function UnitEventOnLoad () returns boolean
	integer pdex = udg_UDex
	integer i = GetUnitUserData(GetTriggerUnit())
	integer index
	if i != 0 
		udg_UDex = i
		if udg_CargoTransportUnit[i] != null 
			UnitEventOnUnload()
		
		//Loaded corpses do not issue an order when unloaded, therefore must
		//use the enter-region event method taken from_ Jesus4Lyf's Transport.
		if not udg_IsUnitAlive[i] 
			SetUnitX(udg_UDexUnits[i], udg_WorldMaxX)
			SetUnitY(udg_UDexUnits[i], udg_WorldMaxY)
		
		
		udg_CargoTransportUnit[i] = GetTransportUnit()
		index = GetUnitUserData(udg_CargoTransportUnit[i])
		if udg_CargoTransportGroup[index] == null 
			udg_CargoTransportGroup[index] = CreateGroup()
		
		GroupAddUnit(udg_CargoTransportGroup[index], udg_UDexUnits[i])
		udg_CargoEvent = 0.00
		udg_CargoEvent = 1.00
		udg_CargoEvent = 0.00
		udg_UDex = pdex
	
	return false

public function UnitEventEnter () returns boolean
	integer pdex = udg_UDex
	integer i = udg_UDexLastRecycled
	unit u = GetFilterUnit()
	if udg_UnitIndexerEnabled and GetUnitAbilityLevel(u, udg_DetectRemoveAbility) == 0 
		//Generate a unique integer index for_ this_ unit
		if i == 0 
			i = udg_UDexMax + 1
			udg_UDexMax = i
		else
			udg_UDexLastRecycled = udg_UDexPrev[i]
		
		//Link index to_ unit, unit to_ index
		udg_UDexUnits[i] = u
		SetUnitUserData(u, i)
		
		//For backwards-compatibility, add the unit to_ a linked list
		udg_UDexNext[i] = udg_UDexNext[0]
		udg_UDexPrev[udg_UDexNext[0]] = i
		udg_UDexNext[0] = i
		udg_UDexPrev[i] = 0
		
		udg_CheckDeathInList[i] = false
		
		UnitAddAbility(u, udg_DetectRemoveAbility)
		UnitMakeAbilityPermanent(u, true, udg_DetectRemoveAbility)
		UnitAddAbility(u, udg_DetectTransformAbility)
		udg_UnitTypeOf[i] = GetUnitTypeId(u)
		udg_IsUnitNew[i] = true
		udg_IsUnitAlive[i] = true
		udg_IsUnitRemoved[i] = false
		udg_IsUnitReincarnating[i] = false
		udg_IsUnitPreplaced[i] = udg_IsUnitPreplaced[0] //Added 29 May 2017 for_ Spellbound
		UnitEventCheckAfterProxy(i)
		
		//Fire index event for_ UDex
		udg_UDex = i
		udg_UnitIndexEvent = 0.00
		udg_UnitIndexEvent = 1.00
		udg_UnitIndexEvent = 0.00
	else
		udg_UDex = GetUnitUserData(u)
		if udg_CargoTransportUnit[udg_UDex] != null and not IsUnitLoaded(u) 
			//The unit was dead, but has re-entered the map.
			UnitEventOnUnload()
		
	
	udg_UDex = pdex
	u = null
	return false

