	vel=vel*SlashStrike__TIMER_MOVE_DELAY
	x=x+vel*Cos(rad)
	y=y+vel*Sin(rad)
	SetUnitX(u,x)
	SetUnitY(u,y)
	z=z+(s__SlashStrike__PhysicsUnitForce[array2d]*SlashStrike__TIMER_MOVE_DELAY)+(s__SlashStrike__PhysicsFloorZ[array2d]-GetUnitFlyHeight(u))
	SetUnitFlyHeight(u,z,SlashStrike__REAL_NULL)
	s__SlashStrike__PhysicsFloorZ[array2d]=GetUnitFlyHeight(u)
	if(GetUnitFlyHeight(u)<=.333)
		SetUnitFlyHeight(u,SlashStrike__REAL_NULL,SlashStrike__REAL_NULL)
		GroupRemoveUnit(s__SlashStrike__BlowedUnitsGroup[SlashStrike__TempId],u)
		if(IsTerrainPathable(x,y,PATHING_TYPE_FLOATABILITY))
			DestroyEffect(AddSpecialEffect(SlashStrike__DUST_EFFECT,x,y))
		else
			DestroyEffect(AddSpecialEffect(SlashStrike__WATER_EFFECT,x,y))
		
	

public function SlashStrike__SlashStrikeCond () returns boolean
	return(GetSpellAbilityId()==SlashStrike__ABILITY_SLASH_STRIKE)

public function SlashStrike__SlashStrikePeriodicMove ()
	trigger this_=GetTriggeringTrigger()
	integer id=s__SlashStrike__StoredValue[QMod(GetHandleId(this_)-0xFFFFF,128)]
	unit u=s__SlashStrike__Caster[id]
	real tx=s__SlashStrike__TargetPosX[id]
	real ty=s__SlashStrike__TargetPosY[id]
	real x=GetUnitX(u)
	real y=GetUnitY(u)
	real tdist=SquareRoot(SlashStrike__Pow2(tx-x)+SlashStrike__Pow2(ty-y))
	real rad=s__SlashStrike__CasterRadians[id]
	fogmodifier fm=null
	unit illusion=null
	real dr_minx=SlashStrike__REAL_NULL
	real dr_miny=SlashStrike__REAL_NULL
	real dr_maxx=SlashStrike__REAL_NULL
	real dr_maxy=SlashStrike__REAL_NULL
	real bwx=SlashStrike__REAL_NULL
	real bwy=SlashStrike__REAL_NULL
	real ax
	real ay
	real bx
	real by
	real cx
	real cy
	real dx
	real dy
	real cos
	real sin
	s__SlashStrike__TimerExec[id]=s__SlashStrike__TimerExec[id]+1
	if(s__SlashStrike__PushTarget[id])
		tx=GetUnitX(s__SlashStrike__TargetUnit[id])+SlashStrike__TARGET_BLOW_SPEED*SlashStrike__TIMER_MOVE_DELAY*Cos(rad)
		ty=GetUnitY(s__SlashStrike__TargetUnit[id])+SlashStrike__TARGET_BLOW_SPEED*SlashStrike__TIMER_MOVE_DELAY*Sin(rad)
		s__SlashStrike__CasterDistance[id]=s__SlashStrike__CasterDistance[id]-(SlashStrike__TARGET_BLOW_SPEED*SlashStrike__TIMER_MOVE_DELAY)
		if(QMod(s__SlashStrike__TimerExec[id],25)==SlashStrike__INT_NULL)
			if(IsTerrainPathable(tx,ty,PATHING_TYPE_FLOATABILITY))
				DestroyEffect(AddSpecialEffect(SlashStrike__DUST_EFFECT,tx,ty))
			else
				DestroyEffect(AddSpecialEffect(SlashStrike__WATER_EFFECT,tx,ty))
			
		
		SetUnitPosition(s__SlashStrike__TargetUnit[id],tx,ty)
		if(s__SlashStrike__CasterDistance[id]<=SlashStrike__REAL_NULL)
			s__SlashStrike__PushTarget[id]=false
			PauseUnit(s__SlashStrike__TargetUnit[id],false)
			SetUnitTimeScale(s__SlashStrike__TargetUnit[id],1.)
		
	
	if(GetUnitState(u,UNIT_STATE_LIFE)<=SlashStrike__REAL_NULL and u!=null)
		SlashStrike__EndSlashStrike()
		return
	
	rad=-SlashStrike__PI_HALF+rad
	cos=Cos(rad)
	sin=Sin(rad)
	bwx=x+SlashStrike__SS_BLADE_WIDTH*cos
	bwy=y+SlashStrike__SS_BLADE_WIDTH*sin
	ax=x+SlashStrike__SS_BLADE_HEIGHT*0.5*Cos(SlashStrike__PI_HALF+rad)
	ay=y+SlashStrike__SS_BLADE_HEIGHT*0.5*Sin(SlashStrike__PI_HALF+rad)
	cx=ax+SlashStrike__SS_BLADE_WIDTH*cos
	cy=ay+SlashStrike__SS_BLADE_WIDTH*sin
	bx=ax+SlashStrike__SS_BLADE_HEIGHT*Cos(-SlashStrike__PI_HALF+rad)
	by=ax+SlashStrike__SS_BLADE_HEIGHT*Sin(-SlashStrike__PI_HALF+rad)
	dx=bx+SlashStrike__SS_BLADE_WIDTH*cos
	dy=by+SlashStrike__SS_BLADE_WIDTH*sin
	dr_minx=ax
	dr_maxx=ax
	if(dr_minx>bx)
		dr_minx=bx
	
	if(dr_maxx<bx)
		dr_maxx=bx
	
	if(dr_minx>cx)
		dr_minx=cx
	
	if(dr_maxx<cx)
		dr_maxx=cx
	
	if(dr_minx>dx)
		dr_minx=dx
	
	if(dr_maxx<dx)
		dr_maxx=dx
	
	dr_miny=ay
	dr_maxy=ay
	if(dr_miny>by)
		dr_miny=by
	
	if(dr_maxy<by)
		dr_maxy=by
	
	if(dr_miny>cy)
		dr_miny=cy
	
	if(dr_maxy<cy)
		dr_maxy=cy
	
	if(dr_miny>dy)
		dr_miny=dy
	
	if(dr_maxy<dy)
		dr_maxy=dy
	
	SlashStrike__TempId=id
	if(s__SlashStrike__NQuickSlashes[id]!=SlashStrike__ERROR)
		SetRect(s__SlashStrike__DamageRect[id],dr_minx,dr_miny,dr_maxx,dr_maxy)
	else
		SetRect(s__SlashStrike__DamageRect[id],SlashStrike__REAL_NULL,SlashStrike__REAL_NULL,SlashStrike__REAL_NULL,SlashStrike__REAL_NULL)
	
	GroupEnumUnitsInRect(s__SlashStrike__DamagedUnitsGroup[id],s__SlashStrike__DamageRect[id],s__SlashStrike__DamageFilter[id])
	ForGroup(s__SlashStrike__DamagedUnitsGroup[id],function SlashStrike__DamageEnumUnit)
	rad=SlashStrike__PI_HALF+rad
	if(SlashStrike__ILLUSION_COUNTER!=SlashStrike__ERROR)
		if(QMod(s__SlashStrike__TimerExec[id],R2I(1/ SlashStrike__TIMER_MOVE_DELAY / SlashStrike__ILLUSION_COUNTER))==SlashStrike__INT_NULL and u!=null and s__SlashStrike__NQuickSlashes[id]!=SlashStrike__ERROR)
			illusion=CreateUnit(SlashStrike__PLAYER_NP,SlashStrike__SS_CASTER_IMAGE,x,y,rad*bj_RADTODEG)
			SetUnitTimeScale(illusion,GetRandomReal(SlashStrike__ANIM_MIN_POS,SlashStrike__ANIM_MAX_POS)/ SlashStrike__TIMER_MOVE_DELAY)
			SetUnitAnimation(illusion,SlashStrike__CASTER_SPELL_ANIMATION)
			SetUnitColor(illusion,PLAYER_COLOR_LIGHT_BLUE)
			UnitAddAbility(illusion,SlashStrike__ABILITY_LOCUST)
			SetUnitVertexColor(illusion,SlashStrike__MAX_HEX_RGB,SlashStrike__MAX_HEX_RGB,SlashStrike__MAX_HEX_RGB,128)
			SetUnitUserData(illusion,128)
			GroupAddUnit(s__SlashStrike__IllusionGroup[id],illusion)
			SetUnitX(illusion,x)
			SetUnitY(illusion,y)
		
		ForGroup(s__SlashStrike__IllusionGroup[id],function SlashStrike__IllusionsFadeIn)
	
	if(SlashStrike__USE_PHYSICS_ENGINE)
		if(FirstOfGroup(s__SlashStrike__BlowedUnitsGroup[id])!=null)
			ForGroup(s__SlashStrike__BlowedUnitsGroup[id],function SlashStrike__PhysicsBlowUnit)
		
	
	if(u==null or s__SlashStrike__NQuickSlashes[id]==SlashStrike__ERROR)
		return
	
	if(SlashStrike__CAN_KILL_TRESS)
		SetRect(SlashStrike__DEST_RECT,x-100.,y-100.,x+100.,y+100.)
		EnumDestructablesInRect(SlashStrike__DEST_RECT,s__SlashStrike__DestFilter[id],function SlashStrike__KillEnumDestructables)
	
	if(IsTerrainPathable(x,y,PATHING_TYPE_FLOATABILITY))
		if(QMod(s__SlashStrike__TimerExec[id],17)==SlashStrike__INT_NULL)
			DestroyEffect(AddSpecialEffect(SlashStrike__DUST_EFFECT,x,y))
		
	else
		if(QMod(s__SlashStrike__TimerExec[id],6)==SlashStrike__INT_NULL)
			DestroyEffect(AddSpecialEffect(SlashStrike__WATER_EFFECT,x,y))
		
	
	if(s__SlashStrike__QuickSlash[id])
		if(s__SlashStrike__NQuickSlashes[id]>SlashStrike__INT_NULL)
			x=x+SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY*s__SlashStrike__ConstCos[id]
			y=y+SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY*s__SlashStrike__ConstSin[id]
			if(SquareRoot(SlashStrike__Pow2(tx-x)+SlashStrike__Pow2(ty-y))<=SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY or SquareRoot(SlashStrike__Pow2(GetUnitX(s__SlashStrike__TargetUnit[id])-GetUnitX(u))+SlashStrike__Pow2(GetUnitY(s__SlashStrike__TargetUnit[id])-GetUnitY(u)))>=SlashStrike__SS_SLASH_RADIUS*1.5)
				s__SlashStrike__NQuickSlashes[id]=s__SlashStrike__NQuickSlashes[id]-1
				tdist=GetRandomReal(SlashStrike__SS_SLASH_RADIUS/ 2.,SlashStrike__SS_SLASH_RADIUS*1.5)
				rad=GetRandomReal(SlashStrike__REAL_NULL,SlashStrike__bj_2PI)
				tx=GetUnitX(s__SlashStrike__TargetUnit[id])
				ty=GetUnitY(s__SlashStrike__TargetUnit[id])
				tx=tx+tdist*Cos(rad)
				ty=ty+tdist*Sin(rad)
				s__SlashStrike__TargetPosX[id]=tx
				s__SlashStrike__TargetPosY[id]=ty
				s__SlashStrike__CasterRadians[id]=Atan2(ty-y,tx-x)
				s__SlashStrike__ConstCos[id]=Cos(s__SlashStrike__CasterRadians[id])
				s__SlashStrike__ConstSin[id]=Sin(s__SlashStrike__CasterRadians[id])
				SetUnitFacing(u,s__SlashStrike__CasterRadians[id]*bj_RADTODEG)
				if(s__SlashStrike__NQuickSlashes[id]==SlashStrike__INT_NULL)
					SetUnitTimeScale(u,2.)
					SetUnitAnimationByIndex(u,SlashStrike__CASTER_QSLASH_ANIM_INDEX)
				
			
		else if(s__SlashStrike__NQuickSlashes[id]==SlashStrike__INT_NULL)
			tx=GetUnitX(s__SlashStrike__TargetUnit[id])
			ty=GetUnitY(s__SlashStrike__TargetUnit[id])
			rad=Atan2(ty-y,tx-x)
			SetUnitFacing(u,rad*bj_RADTODEG)
			s__SlashStrike__CasterRadians[id]=rad
			x=x+SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY*Cos(rad)
			y=y+SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY*Sin(rad)
			if(SquareRoot(SlashStrike__Pow2(tx-x)+SlashStrike__Pow2(ty-y))<=2.*SlashStrike__CASTER_COLLISION_SIZE)
				s__SlashStrike__NQuickSlashes[id]=SlashStrike__ERROR
				SetUnitInvulnerable(u,true)
				fm=CreateFogModifierRadius(GetOwningPlayer(u),FOG_OF_WAR_VISIBLE,tx,ty,512.,true,false)
				FogModifierStart(fm)
				ShowUnit(u,false)
				PauseUnit(s__SlashStrike__TargetUnit[id],true)
				SetUnitTimeScale(s__SlashStrike__TargetUnit[id],SlashStrike__REAL_NULL)
				DestroyEffect(AddSpecialEffect(SlashStrike__BLINK_CASTER_QModEL,x,y))
				Sleep(GetRandomReal(0.5,0.75))
				ShowUnit(u,true)
				rad=GetRandomReal(SlashStrike__REAL_NULL,SlashStrike__bj_2PI)
				x=tx+SlashStrike__QSLASH_TARGET_DISTANCE*Cos(rad)
				y=ty+SlashStrike__QSLASH_TARGET_DISTANCE*Sin(rad)
				SetUnitX(u,x)
				SetUnitY(u,y)
				rad=Atan2(ty-y,tx-x)
				SetUnitFacing(u,rad*bj_RADTODEG)
				DestroyEffect(AddSpecialEffect(SlashStrike__BLINK_TARGET_QModEL,x,y))
				if(not IsUnitType(s__SlashStrike__TargetUnit[id],UNIT_TYPE_DEAD))
					SetUnitAnimationByIndex(u,SlashStrike__CASTER_QSLASH_ANIM_INDEX)
					Sleep(SlashStrike__MIN_SLEEP)
					if(SlashStrike__USE_PHYSICS_ENGINE)
						s__SlashStrike__CasterDistance[id]=250.
						s__SlashStrike__CasterRadians[id]=rad
						s__SlashStrike__PushTarget[id]=true
					
					UnitDamageTarget(u,s__SlashStrike__TargetUnit[id],s__SlashStrike__Damage[s__SlashStrike__SS_SpellLevel[id]],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_UNIVERSAL,WEAPON_TYPE_METAL_HEAVY_SLICE)
					DestroyEffect(AddSpecialEffectTarget(SlashStrike__BLOOD_EFFECT,s__SlashStrike__TargetUnit[id],SlashStrike__ATTACH_POINT_ORIGIN))
				
				SetUnitInvulnerable(u,false)
				SetUnitTimeScale(s__SlashStrike__TargetUnit[id],1.)
				if(GetLocalPlayer()==GetOwningPlayer(u))
					SelectUnit(u,true)
				
				Sleep(SlashStrike__CASTER_QSLASH_ANIM_DURATION/ 2.-0.25)
				FogModifierStop(fm)
				DestroyFogModifier(fm)
				fm=null
				SlashStrike__EndSlashStrike()
				return
			
		
		SetUnitPosition(u,x,y)
		return
	
	if(tdist<=100. and not s__SlashStrike__CircleMove[id])
		SetUnitPosition(u,tx,ty)
		x=tx
		y=ty
		rad=rad+SlashStrike__PI_HALF
		s__SlashStrike__CircleMove[id]=true
		s__SlashStrike__TargetPosX[id]=tx+SlashStrike__SS_SLASH_RADIUS*Cos(rad)
		s__SlashStrike__TargetPosY[id]=ty+SlashStrike__SS_SLASH_RADIUS*Sin(rad)
		if(s__SlashStrike__CasterRadians[id]==s__SlashStrike__BackRadians[id])
			SlashStrike__EndSlashStrike()
		
		return
	else if(not s__SlashStrike__CircleMove[id])
		x=x+SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY*s__SlashStrike__ConstCos[id]
		y=y+SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY*s__SlashStrike__ConstSin[id]
		if(IsTerrainPathable(x+SlashStrike__CASTER_COLLISION_SIZE*SlashStrike__TIMER_MOVE_DELAY*s__SlashStrike__ConstCos[id],y+SlashStrike__CASTER_COLLISION_SIZE*SlashStrike__TIMER_MOVE_DELAY*s__SlashStrike__ConstSin[id],PATHING_TYPE_WALKABILITY))
			SlashStrike__EndSlashStrike()
			return
		
		SetUnitPosition(u,x,y)
	else
		s__SlashStrike__CasterRadians[id]=rad+(SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY)*(1./ SlashStrike__SS_SLASH_RADIUS)
		SetUnitFacing(u,rad*bj_RADTODEG)
		rad=Atan2(y-s__SlashStrike__TargetPosY[id],x-s__SlashStrike__TargetPosX[id])+(SlashStrike__CASTER_MOVE_SPEED*SlashStrike__TIMER_MOVE_DELAY)*(1./ SlashStrike__SS_SLASH_RADIUS)
		x=s__SlashStrike__TargetPosX[id]+SlashStrike__SS_SLASH_RADIUS*Cos(rad)
		y=s__SlashStrike__TargetPosY[id]+SlashStrike__SS_SLASH_RADIUS*Sin(rad)
		if(IsTerrainPathable(x+SlashStrike__CASTER_COLLISION_SIZE*Cos(rad),y+SlashStrike__CASTER_COLLISION_SIZE*Sin(rad),PATHING_TYPE_WALKABILITY))
			SlashStrike__EndSlashStrike()
			return
		
		SetUnitPosition(u,x,y)
		if(s__SlashStrike__CasterRadians[id]>=s__SlashStrike__BackRadians[id])
			s__SlashStrike__CasterRadians[id]=s__SlashStrike__BackRadians[id]
			s__SlashStrike__CircleMove[id]=false
			s__SlashStrike__CasterRadians[id]=s__SlashStrike__BackRadians[id]
			s__SlashStrike__ConstCos[id]=Cos(s__SlashStrike__BackRadians[id])
			s__SlashStrike__ConstSin[id]=Sin(s__SlashStrike__BackRadians[id])
			s__SlashStrike__TargetPosX[id]=x+s__SlashStrike__CasterDistance[id]*s__SlashStrike__ConstCos[id]
			s__SlashStrike__TargetPosY[id]=y+s__SlashStrike__CasterDistance[id]*s__SlashStrike__ConstSin[id]
		
	

public function SlashStrike__SlashStrikeAction ()
	unit u=GetTriggerUnit()
	unit v=GetSpellTargetUnit()
	player p=GetOwningPlayer(u)
	real x1=GetUnitX(u)
	real x2=GetUnitX(v)
	real y1=GetUnitY(u)
	real y2=GetUnitY(v)
	real dist=SquareRoot(SlashStrike__Pow2(x1-x2)+SlashStrike__Pow2(y1-y2))
	real rad=Atan2(y2-y1,x2-x1)
	real x=SlashStrike__REAL_NULL
	real y=SlashStrike__REAL_NULL
	integer id=SlashStrike__NCast
	trigger this_=null
	integer lvl=GetUnitAbilityLevel(u,SlashStrike__ABILITY_SLASH_STRIKE)
	if(lvl>SlashStrike__Levels)
		DisplayTimedTextToPlayer(GetLocalPlayer(),SlashStrike__REAL_NULL,SlashStrike__REAL_NULL,15.,"|cffff0000Spell: Slash Strike can't be cast because level of spell is greater then defined.|r")
		StartSound(SlashStrike__ERROR_SOUND)
		SetSoundVolume(SlashStrike__ERROR_SOUND,127)
		return
	
	SlashStrike__NCast=QMod(SlashStrike__NCast+1,128)
	if(dist>SlashStrike__SS_SLASH_RADIUS+SlashStrike__DISTANCE_FROM_TARGET)
		rad=rad-SlashStrike__PI_HALF
		x=x1+SlashStrike__SS_SLASH_RADIUS*Cos(rad)
		y=y1+SlashStrike__SS_SLASH_RADIUS*Sin(rad)
		rad=rad+SlashStrike__PI_HALF
		s__SlashStrike__CasterDistance[id]=(dist-SlashStrike__SS_SLASH_RADIUS-SlashStrike__DISTANCE_FROM_TARGET)
		s__SlashStrike__TargetPosX[id]=x+s__SlashStrike__CasterDistance[id]*Cos(rad)
		s__SlashStrike__TargetPosY[id]=y+s__SlashStrike__CasterDistance[id]*Sin(rad)
		s__SlashStrike__CircleMove[id]=false
		s__SlashStrike__QuickSlash[id]=false
		s__SlashStrike__NQuickSlashes[id]=0
	else
		rad=GetRandomReal(SlashStrike__REAL_NULL,SlashStrike__bj_2PI)
		dist=GetRandomReal(SlashStrike__SS_SLASH_RADIUS/ 2.,SlashStrike__SS_SLASH_RADIUS*1.5)
		x=x2+dist*Cos(rad)
		y=y2+dist*Sin(rad)
		rad=GetRandomReal(SlashStrike__REAL_NULL,SlashStrike__bj_2PI)
		SetRandomSeed(R2I(GetRandomReal(0.,rad*10000.)))
		dist=GetRandomReal(SlashStrike__SS_SLASH_RADIUS/ 2.,SlashStrike__SS_SLASH_RADIUS*1.5)
		s__SlashStrike__TargetPosX[id]=x2+dist*Cos(rad)
		s__SlashStrike__TargetPosY[id]=y2+dist*Sin(rad)
		rad=Atan2(s__SlashStrike__TargetPosY[id]-y,s__SlashStrike__TargetPosX[id]-x)
		s__SlashStrike__NQuickSlashes[id]=s__SlashStrike__QuickSlashes[lvl]
		s__SlashStrike__TargetUnit[id]=v
		s__SlashStrike__QuickSlash[id]=true
	
	if(IsTerrainPathable(x+SlashStrike__CASTER_COLLISION_SIZE*Cos(rad),y+SlashStrike__CASTER_COLLISION_SIZE*Sin(rad),PATHING_TYPE_WALKABILITY))
		if(GetLocalPlayer()==p)
			SetSoundVolume(SlashStrike__ERROR_SOUND,110)
			StartSound(SlashStrike__ERROR_SOUND)
		
		DisplayTimedTextToPlayer(p,SlashStrike__REAL_NULL,SlashStrike__REAL_NULL,15.,"|cffffcc00Invalid cast position!|r")
		return
	
	UnitAddAbility(u,SlashStrike__CROW_FORM_RAWCODE)
	UnitRemoveAbility(u,SlashStrike__CROW_FORM_RAWCODE)
	SetUnitInvulnerable(u,true)
	DestroyEffect(AddSpecialEffect(SlashStrike__BLINK_CASTER_QModEL,x1,y1))
	Sleep(SlashStrike__MIN_SLEEP)
	DestroyEffect(AddSpecialEffect(SlashStrike__BLINK_TARGET_QModEL,x,y))
	SetUnitInvulnerable(u,false)
	SetUnitPosition(u,x,y)
	SetUnitFacing(u,rad*bj_RADTODEG)
	PauseUnit(u,true)
	SetUnitAnimation(u,SlashStrike__CASTER_SPELL_ANIMATION)
	SetUnitTimeScale(u,1.78324)
	Sleep(SlashStrike__CASTER_READY_DURATION/ 1.78324-0.25)
	SetUnitTimeScale(u,.0316)
	if(GetUnitState(u,UNIT_STATE_LIFE)<=SlashStrike__REAL_NULL)
		PauseUnit(u,false)
		SetUnitTimeScale(u,1.)
		return
	
	SlashStrike__TriggerCount=SlashStrike__TriggerCount+1
	this_=CreateTrigger()
	s__SlashStrike__StoredValue[QMod(GetHandleId(this_)-0xFFFFF,128)]=id
	s__SlashStrike__ConstSin[id]=Sin(rad)
	s__SlashStrike__ConstCos[id]=Cos(rad)
	s__SlashStrike__BackRadians[id]=rad+bj_PI
	s__SlashStrike__TimerExec[id]=SlashStrike__INT_NULL
	s__SlashStrike__PushTarget[id]=false
	s__SlashStrike__CasterRadians[id]=rad
	s__SlashStrike__Caster[id]=u
	if(SlashStrike__ILLUSION_COUNTER!=SlashStrike__ERROR)
		s__SlashStrike__IllusionGroup[id]=CreateGroup()
	
	if(SlashStrike__USE_PHYSICS_ENGINE)
		s__SlashStrike__BlowedUnitsGroup[id]=CreateGroup()
	
	if(SlashStrike__CAN_KILL_TRESS)
		s__SlashStrike__DestFilter[id]=Filter(function SlashStrike__IsDestAlive)
	
	s__SlashStrike__DamagedUnitsGroup[id]=CreateGroup()
	s__SlashStrike__DamageFilter[id]=Filter(function SlashStrike__DamageFilterFunc)
	s__SlashStrike__DamageRect[id]=Rect(SlashStrike__REAL_NULL,SlashStrike__REAL_NULL,SlashStrike__REAL_NULL,SlashStrike__REAL_NULL)
	s__SlashStrike__SS_SpellLevel[id]=lvl
	if(SlashStrike__SSTimer==null)
		SlashStrike__SSTimer=CreateTimer()
		TimerStart(SlashStrike__SSTimer,SlashStrike__TIMER_MOVE_DELAY,TRUE,null)
	
	TriggerRegisterTimerExpireEvent(this_,SlashStrike__SSTimer)
	s__SlashStrike__TAction[id]=TriggerAddAction(this_,function SlashStrike__SlashStrikePeriodicMove)
	SetUnitPathing(u,false)
	p=null
	u=null
	v=null
	this_=null

public function SlashStrike__SlashStrike_Main ()
	trigger t=CreateTrigger()
	sound s=null
	TriggerRegisterAnyUnitEvent(t,EVENT_PLAYER_UNIT_SPELL_EFFECT)
	TriggerAddCondition(t,Condition(function SlashStrike__SlashStrikeCond))
	TriggerAddAction(t,function SlashStrike__SlashStrikeAction)
	SetSoundVolume(SlashStrike__ERROR_SOUND,1)
	StartSound(SlashStrike__ERROR_SOUND)
	if(SlashStrike__CAN_KILL_TRESS)
