public function Crush___UnitAddBuff (unit victim,real duration,integer lvl )
	integer index
	if s__Crush___BuffData_size==0 
		TimerStart(s__Crush___BuffData_loopExe,0.5,true,function s__Crush___BuffData_BuffCheck)
	
	if GetUnitAbilityLevel(victim,Crush___BuffAddId)>0 
		index=(LoadInteger(HIS__tempcache,GetHandleId((victim)),HIS__pos))
		UnitRemoveAbility(victim,Crush___BuffAddId)
	else
		AddHandleIndex(victim)
		index=(LoadInteger(HIS__tempcache,GetHandleId((victim)),HIS__pos))
		s__s__Crush___BuffData_runStruct[index]=s__Crush___BuffData__allocate()
		s__s__Crush___BuffData_executer[s__Crush___BuffData_size]=index
		s__Crush___BuffData_size=s__Crush___BuffData_size+1
	
	s__Crush___BuffData_victim[s__s__Crush___BuffData_runStruct[index]]=victim
	s__Crush___BuffData_duration[s__s__Crush___BuffData_runStruct[index]]=duration
	s__Crush___BuffData_time[s__s__Crush___BuffData_runStruct[index]]=0.
	UnitAddAbility(victim,Crush___BuffAddId)
	SetUnitAbilityLevel(victim,Crush___BuffAddId,lvl)

public function s__Crush___CrushDatas_motion (integer this_ )
	real x=GetUnitX(s__Crush___CrushDatas_attacked[this_])+Crush___MotionSpeed*Cos(s__Crush___CrushDatas_angle[this_])
	real y=GetUnitY(s__Crush___CrushDatas_attacked[this_])+Crush___MotionSpeed*Sin(s__Crush___CrushDatas_angle[this_])
	if RectContainsCoords(Crush___MaxArea,x,y)and IsPointWalkable(x,y)
		SetUnitX(s__Crush___CrushDatas_attacked[this_],x)
		SetUnitY(s__Crush___CrushDatas_attacked[this_],y)
	
	DestroyEffect(AddSpecialEffectTarget(Crush___PushMdl,s__Crush___CrushDatas_attacked[this_],Crush___PushAtt))
	s__Crush___CrushDatas_tt[this_]=Crush___periodic+s__Crush___CrushDatas_tt[this_]
	if s__Crush___CrushDatas_tt[this_]>Crush___MaxPushTime 
		s__Crush___CrushDatas_active[this_]=false
	

public function s__Crush___CrushDatas_endmotion (integer this_ )
	s__Crush___CrushDatas_attacked[this_]=null
	s__Crush___CrushDatas_deallocate(this_)

public function s__Crush___CrushDatas_loopRun ()
	integer index=0
	while true
		if index==s__Crush___CrushDatas_size
			break
		if not s__Crush___CrushDatas_paused[s__s__Crush___CrushDatas_runStruct[index]]
			if s__Crush___CrushDatas_active[s__s__Crush___CrushDatas_runStruct[index]]
				s__Crush___CrushDatas_motion(s__s__Crush___CrushDatas_runStruct[index])
				index=index+1
			else
				s__Crush___CrushDatas_endmotion(s__s__Crush___CrushDatas_runStruct[index])
				s__Crush___CrushDatas_size=s__Crush___CrushDatas_size-1
				s__s__Crush___CrushDatas_runStruct[index]=s__s__Crush___CrushDatas_runStruct[s__Crush___CrushDatas_size]
			
		else
			index=index+1
		
	
	if s__Crush___CrushDatas_size==0 
		PauseTimer(s__Crush___CrushDatas_loopExe)
	

public function s__Crush___CrushDatas_addMotion (integer data ) returns integer
	if s__Crush___CrushDatas_size==0 
		TimerStart(s__Crush___CrushDatas_loopExe,Crush___periodic,true,function s__Crush___CrushDatas_loopRun)
	
	s__s__Crush___CrushDatas_runStruct[s__Crush___CrushDatas_size]=data
	s__Crush___CrushDatas_active[s__s__Crush___CrushDatas_runStruct[s__Crush___CrushDatas_size]]=true
	s__Crush___CrushDatas_paused[s__s__Crush___CrushDatas_runStruct[s__Crush___CrushDatas_size]]=false
	s__Crush___CrushDatas_size=s__Crush___CrushDatas_size+1
	return s__Crush___CrushDatas_size

public function Crush___CrushBegin () returns boolean
	integer dat=s__Crush___CrushDatas__allocate()
	integer lvl=GetUnitAbilityLevel(DamageSkill_attacked,Crush___SpellId)
	Crush___UnitAddBuff(DamageSkill_attacked,Crush___BuffDuration(lvl),lvl)
	s__Crush___CrushDatas_tt[dat]=0.
	s__Crush___CrushDatas_attacked[dat]=DamageSkill_attacked
	s__Crush___CrushDatas_angle[dat]=Atan2(GetUnitY(DamageSkill_attacked)-GetUnitY(DamageSkill_attacker),GetUnitX(DamageSkill_attacked)-GetUnitX(DamageSkill_attacker))
	s__Crush___CrushDatas_addMotion(dat)
	return true

