// Beast Master Summons
public function RK_BeastMaster_Scaling_Logic () returns boolean
	unit caster = GetSummoningUnit()
	unit spawn = GetSummonedUnit()
	real maxHp = 0
	real newHp = 0
	real currentDmg = 0
	real newDmg = 0
	real currentArmr = 0
	real newArmr = 0
	
	if (GetUnitTypeId(caster) != 'H115' and GetUnitTypeId(caster) != 'H115') 
		return false
	
	
	maxHp = I2R(BlzGetUnitMaxHP(spawn))
	currentDmg = I2R(BlzGetUnitBaseDamage(spawn, 0))
	currentArmr = BlzGetUnitArmor(spawn)
	
	if (GetUnitName(spawn) == "Misha Bear") 
		newHp = 0.5 * I2R(GetHeroStr(caster, true))
		newArmr = 0.5 * BlzGetUnitArmor(caster)
		newDmg = 0.25 * I2R(GetHeroStr(caster, true))
	
	
	if (GetUnitName(spawn) == "Quilbeast") 
		newHp = 0.25 * I2R(GetHeroStr(caster, true))
		newArmr = 0.25 * BlzGetUnitArmor(caster)
		newDmg = 0.5 * I2R(GetHeroStr(caster, true))
	
	
	if (GetUnitName(spawn) == "Hawk") 
		newHp = 0.1 * I2R(GetHeroStr(caster, true))
		newArmr = 0.1 * BlzGetUnitArmor(caster)
		newDmg = 1.0 * I2R(GetHeroStr(caster, true))
	
	
	if newHp <= 0 
		return false
	
	
	newHp = newHp + maxHp * 0.04
	BlzSetUnitMaxHP(spawn, R2I(newHp) * 25)
	SetUnitLifePercentBJ(spawn, 100)
	
	newArmr = newArmr + currentArmr
	BlzSetUnitArmor(spawn, newArmr)
	
	newDmg = newDmg + currentDmg
	BlzSetUnitBaseDamage(spawn, R2I(newDmg), 0)
	
	
	caster = null
	spawn = null
	return false

public function RK_BeastMaster_Scaling_Init ()
	gg_trg_RK_BeastMaster_Scaling = CreateTrigger()
	TriggerRegisterAnyUnitEventBJ(gg_trg_RK_BeastMaster_Scaling, EVENT_PLAYER_UNIT_SUMMON)
	TriggerAddCondition(gg_trg_RK_BeastMaster_Scaling, Condition(function RK_BeastMaster_Scaling_Logic))

