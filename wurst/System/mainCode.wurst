	bj_forLoopAIndexEnd=udg_Cleaverduration
	while true
		if bj_forLoopAIndex>bj_forLoopAIndexEnd
			break
		udg_Cleavertargetgroup=GetRandomSubGroup(1,GetUnitsInRangeOfLocMatching(900.00,udg_Cyan_loc,Condition(function Trig_Cleaver_Func006Func001002002003)))
