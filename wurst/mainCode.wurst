public function Encrypt (string s ) returns string
	integer times=3
	string s2 = ""
	while true
		if false
			break
		if times>0 
			s2=Char2CharEncrypt(s)
			times=times-1
		else
			if true
				break
		
	
	return s2

public function Decrypt (string s ) returns string
	integer times=3
	string s2 = s
	while (true)
