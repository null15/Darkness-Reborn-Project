					snd=CreateSound(DarkTorture___SWORD_HIT_PATH,false,false,false,12700,12700,"")
					DarkTorture___SetSoundVolumeByDistance(snd,DarkTorture___SWORD_HIT_PATH_VOL,s__DarkTorture___sword_posX[s],s__DarkTorture___sword_posY[s])
					StartSound(snd)
					KillSoundWhenDone(snd)
					snd=null
				
				if(IsUnitType(s__DarkTorture___sword_Victim[s],UNIT_TYPE_DEAD))
					s__DarkTorture___sword_Victim[s]=null
					return
				
				s__DarkTorture___UnitIsPushed[ArrayId(s__DarkTorture___sword_Victim[s])]=true
				GroupAddUnit(s__DarkTorture___sword_PushGroup[s],s__DarkTorture___sword_Victim[s])
