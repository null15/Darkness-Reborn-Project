		else
			udg_Cleaverduration=0
			SetUnitPositionLoc(udg_Cyan,udg_Cyan_loc)
			SelectUnitAddForPlayer(udg_Cyan,GetOwningPlayer(udg_Cyan))
			SetUnitVertexColorBJ(udg_Cyan,100,100,100,0.00)
			SetUnitInvulnerable(udg_Cyan,false)
		
		bj_forLoopAIndex=bj_forLoopAIndex+1
	
	udg_Cleaverduration=0
	SetUnitPositionLoc(udg_Cyan,udg_Cyan_loc)
	SelectUnitAddForPlayer(udg_Cyan,GetOwningPlayer(udg_Cyan))
	SetUnitVertexColorBJ(udg_Cyan,100,100,100,0.00)
	SetUnitInvulnerable(udg_Cyan,false)

