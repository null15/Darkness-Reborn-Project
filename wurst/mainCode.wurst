public function InstinctFlash__LoadIllusions (real x,real y,real rad )
	integer i = 0
	real w = 0
	real h = 0
	real d = 0
	real acc = 0
	real r = 0
	integer tp=s__dd_parab_create(InstinctFlash__ILLUSION_WIDTH,InstinctFlash__ILLUSION_HEIGHT)
	integer p=s__dd_parab_new()
	unit array u
	while true
		i=i+1
		if(i>=3)
			break
		w=i*(550/ 6.)
		h=300-s__dd_parab_GetHeight(tp,w)
		w=w-550*0.5
		d=SquareRoot(w*w+h*h)
		r=Atan(w/ h)
		u[0]=InstinctFlash__CreateIllusion(x+d*Cos(0+rad),y+d*Sin(0+rad),rad,10.,40.)
		u[1]=InstinctFlash__CreateIllusion(x+d*Cos(0+rad),y+d*Sin(0+rad),rad,10.,40.)

		s__dd_parab_SetScale(p,-w,h)
		acc=InstinctFlash__ConvAcc(InstinctFlash__ILLUSION_SVEL,(s__dd_parab_perimeter[(p)])/ 2.,InstinctFlash__ILLUSION_DURATION)
		PushUnit(u[0],InstinctFlash__ILLUSION_SVEL,acc,InstinctFlash__ILLUSION_SVEL+(acc*InstinctFlash__ILLUSION_DURATION),(s__dd_parab_perimeter[(p)])/ 2.,rad+PI_HALF,0.,ParabolaData(p,0.))
		PushUnit(u[1],-InstinctFlash__ILLUSION_SVEL,acc,-(InstinctFlash__ILLUSION_SVEL+(acc*InstinctFlash__ILLUSION_DURATION)),-(s__dd_parab_perimeter[(p)])/ 2.,rad+PI_HALF,0.,ParabolaData(p,(s__dd_parab_perimeter[(p)])))
	InstinctFlash__CreateIllusion(x,y,rad,10.,40.)
	s__dd_parab_delete(tp)
	s__dd_parab_delete(p)
	u[0]=null
	u[1]=null

