		udg_S_MysteriousMagic_Dummy[udg_S_MysteriousMagic_Mui[1]]=GetLastCreatedUnit()
		GroupAddUnitSimple(GetLastCreatedUnit(),udg_S_MysteriousMagic_UG)
		SetUnitUserData(GetLastCreatedUnit(),1)
