public function IsUnitOnLine (unit u,real bx,real by,real length,real height,real radians ) returns boolean
	real ux=GetUnitX(u)
	real uy=GetUnitY(u)
	real rad=Atan2(uy-by,ux-bx)
	real hyp
	if(rad<radians or rad>radians+PI_HALF)
		return false
	
	hyp=(ux-bx)*(ux-bx)+(uy-by)*(uy-by)
	rad=rad-radians
	if(rad<0.)
		rad=-rad
	
	return(hyp*Cos(rad)<=length*length)and(hyp*Sin(rad)<=height*height)

