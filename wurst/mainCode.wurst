public function Trig_ItemsRecipe_Actions ()
	udg_TempItemU=GetManipulatingUnit()
	udg_TempInt=0
	bj_forLoopAIndex=1
	bj_forLoopAIndexEnd=6
	while true
		if bj_forLoopAIndex>bj_forLoopAIndexEnd
			break
		udg_TempItem=UnitItemInSlotBJ(udg_TempItemU,GetForLoopIndexA())
		udg_TempItemType=GetItemTypeId(udg_TempItem)
		if(Trig_ItemsRecipe_Func004Func003C())
			udg_TempInt=(udg_TempInt+1)
			udg_Recipe[udg_TempInt]=udg_TempItem
		else
		
		bj_forLoopAIndex=bj_forLoopAIndex+1
	
	if(Trig_ItemsRecipe_Func006C())
		bj_forLoopAIndex=1
		bj_forLoopAIndexEnd=4
		while true
			if bj_forLoopAIndex>bj_forLoopAIndexEnd
				break
			UnitRemoveItemSwapped(udg_Recipe[GetForLoopIndexA()],udg_TempItemU)
			RemoveItem(udg_Recipe[GetForLoopIndexA()])
			bj_forLoopAIndex=bj_forLoopAIndex+1
		
		UnitAddItemByIdSwapped('I02H',udg_TempItemU)
		AddSpecialEffectTargetUnitBJ("overhead",udg_TempItemU,"Abilities\\Spells\\Items\\TomeOfRetraining\\TomeOfRetrainingCaster.mdl")
		DestroyEffectBJ(GetLastCreatedEffectBJ())
		CreateTextTagUnitBJ("Ruby Fragments Upgrade...",udg_TempItemU,0,10.00,20.00,50.00,50.00,0)
		SetTextTagPermanentBJ(GetLastCreatedTextTag(),false)
		SetTextTagVelocityBJ(GetLastCreatedTextTag(),64,90)
		SetTextTagFadepointBJ(GetLastCreatedTextTag(),3.00)
		SetTextTagLifespanBJ(GetLastCreatedTextTag(),5.00)
	else
	

