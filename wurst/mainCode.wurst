public function MS_ModifyArmor (unit target,integer armorreduction )
	integer array armor
	integer level1=(GetUnitAbilityLevel((target),('A0ZB')))
	integer level2=(GetUnitAbilityLevel((target),('A0ZD')))
	integer level3=(GetUnitAbilityLevel((target),('A0ZC')))
	real armorreduce=I2R(level1+level2*10+level3*100)
	real armorcalc
	integer tempdivider
	integer tempint
	integer i
	armor[0]=('A0ZC')
	armor[1]=('A0ZD')
	armor[2]=('A0ZB')
	if armorreduce==0 
		UnitAddAbility(target,('A0ZF'))
		armorcalc=armorreduce+armorreduction
		if armorcalc==0 
			UnitRemoveAbility(target,('A0ZF'))
		
		i=0
		tempint=0
		tempdivider=100
		while true
			if i==3
				break
			tempint=R2I(armorcalc/ tempdivider)
			SetUnitAbilityLevel(target,armor[i],tempint+1)
			armorcalc=armorcalc-I2R(tempint*tempdivider)
			tempdivider=R2I(tempdivider/ 10)
			i=i+1
		
	else
		armorreduce=I2R(level1+level2*10+level3*100-111)
		armorcalc=armorreduce+armorreduction
		if armorcalc==0 
