	s___DarkTorture___triggerx_sw[s__DarkTorture___triggerx_sw[s__DarkTorture___triggerx_tx[tx]]+DarkTorture___MaxSwordCount[s__DarkTorture___sword_Level[s]]-ex-s__DarkTorture___triggerx_faliure[tx]]=DarkTorture___NULL
	s=DarkTorture___NULL



public function DarkTorture___CreateSwords ()
	integer tmx=(s__s__DarkTorture___timerx_TX_Ptr[ArrayId(GetExpiredTimer())])
	integer s=DarkTorture___NULL
	real x=s__DarkTorture___timerx_x[tmx]
	real y=s__DarkTorture___timerx_y[tmx]
	real radx=(s__DarkTorture___timerx_rad[tmx]-DarkTorture___PI_HALF)
	sound snd=null
	real d=0.
	real cx=0.
	real cy=0.
	integer i=0
	real sc=I2R(DarkTorture___MaxSwordCount[s__DarkTorture___timerx_lvl[tmx]])
	integer tx=DarkTorture___NULL
	if(s__DarkTorture___timerx_exec[tmx]>=DarkTorture___MaxSwordCount[s__DarkTorture___timerx_lvl[tmx]])
		tx=s__DarkTorture___triggerx_new(function DarkTorture___ShootBlades,s__DarkTorture___timerx_lvl[tmx],s__DarkTorture___timerx_tx[tmx],s__DarkTorture___timerx_faliure[tmx])
		TriggerRegisterTimerEvent(s__DarkTorture___triggerx_t[(tx)],((DarkTorture___SWORD_FIRE_DELAY)*1.0),true)
		TriggerExecute(s__DarkTorture___triggerx_t[(tx)])
		s__DarkTorture___timerx_deallocate(tmx)
		return
	
	radx=radx+(bj_PI/(sc-1))*s__DarkTorture___timerx_exec[tmx]
	x=x+(DarkTorture___SwordLineWidth[s__DarkTorture___timerx_lvl[tmx]]*0.5*Cos(radx))-(DarkTorture___SWORD_LOAD_OFFSET*Cos(s__DarkTorture___timerx_rad[tmx]-PI))
	y=y+(DarkTorture___SwordLineWidth[s__DarkTorture___timerx_lvl[tmx]]*0.5*Sin(radx))-(DarkTorture___SWORD_LOAD_OFFSET*Sin(s__DarkTorture___timerx_rad[tmx]-PI))
	if(not IsTerrainPathable(x,y,PATHING_TYPE_WALKABILITY))
		s=s__DarkTorture___sword_create(s__DarkTorture___timerx_pid[tmx],x,y,DarkTorture___SWORD_HEIGHT,s__DarkTorture___timerx_rad[tmx],s__DarkTorture___timerx_fz[tmx],s__DarkTorture___timerx_lvl[tmx],s__DarkTorture___timerx_tx[tmx])
		s___DarkTorture___triggerx_sw[s__DarkTorture___triggerx_sw[s__DarkTorture___timerx_tx[tmx]]+R2I(s__DarkTorture___timerx_exec[tmx])-s__DarkTorture___timerx_faliure[tmx]]=(s)
		GroupAddUnit(s___DarkTorture___triggerx_g[s__DarkTorture___triggerx_g[s__DarkTorture___timerx_tx[tmx]]],s__DarkTorture___sword_Sword[s])
	else
		// s__DarkTorture___sword_deallocate(s)
		s__DarkTorture___timerx_faliure[tmx]=(s__DarkTorture___timerx_faliure[tmx]+1)
	
	
	snd=CreateSound(DarkTorture___SWORD_CREATE_SOUND_PATH,false,false,false,12700,12700,"")
	DarkTorture___SetSoundVolumeByDistance(snd,DarkTorture___SWORD_CREATE_SOUND_PATH_VOL,x,y)
	StartSound(snd)
	if(s__DarkTorture___timerx_exec[tmx]+5.<DarkTorture___MaxSwordCount[s__DarkTorture___timerx_lvl[tmx]])
		DarkTorture___KillSoundTimed(snd,(DarkTorture___SWORD_CREATION_DELAY*4.)-0.02)
	else
		KillSoundWhenDone(snd)
	
	snd=null
	s__DarkTorture___timerx_exec[tmx]=s__DarkTorture___timerx_exec[tmx]+1.



public function DarkTorture___DarkTorture_Action ()
	unit u=GetTriggerUnit()
	real ux=GetUnitX(u)
	real uy=GetUnitY(u)
	location l=GetSpellTargetLoc()
	real x=GetLocationX(l)
	real y=GetLocationY(l)
	integer lvl=GetUnitAbilityLevel(u,DarkTorture___DARK_TORTURE_RAWCODE)
	real floorz=0.
	integer tx=DarkTorture___NULL
	integer tmx=DarkTorture___NULL
	if(lvl>DarkTorture___Levels)
		BJDebugMsg("|cffff0000The spell Dark Torture canot be executed because spell level is greater then defined!|r")
		h__DestroyTrigger(GetTriggeringTrigger())
		return
	
	floorz=GetUnitFlyHeight(u)
	if(DarkTorture___DTTimer==null)
		DarkTorture___DTTimer=CreateTimer()
		TimerStart(DarkTorture___DTTimer,DarkTorture___TIMER_MIN_DELAY,true,null)
	
	DarkTorture___TGCount=DarkTorture___TGCount+1
	tx=s__DarkTorture___triggerx_create(function DarkTorture___DarkTortureMovement,DarkTorture___SwordsMaxDamage[lvl])
	TriggerRegisterTimerExpireEvent(s__DarkTorture___triggerx_t[tx],DarkTorture___DTTimer)
	tmx=s__DarkTorture___timerx_create(ux,uy,Atan2(y-uy,x-ux),lvl,GetPlayerId(GetOwningPlayer(u)),floorz,tx)
	TimerStart(s__DarkTorture___timerx_t[tmx],DarkTorture___SWORD_CREATION_DELAY,true,function DarkTorture___CreateSwords)
	RemoveLocation(l)
	l=null
	u=null

