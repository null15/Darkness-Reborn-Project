	return(false)

public function DDDamageDetect___Actions ()
	unit u
	integer id
	DisableTrigger(DDDamageDetect___MainTrig)
	EnableTrigger(DDDamageDetect___MainTrig)
	if(GetTriggerEventId()==EVENT_PLAYER_UNIT_SPELL_CAST)
		u=GetTriggerUnit()
		id=ArrayId(u)
		s__DDDamageDetect___NCasts[id]=s__DDDamageDetect___NCasts[id]+1
		doAfter(2.5) ->
			if(s__DDDamageDetect___NCasts[id]>0)
				s__DDDamageDetect___NCasts[id]=s__DDDamageDetect___NCasts[id]-1
		
		u=null
	else if(GetTriggerEventId()==EVENT_PLAYER_UNIT_ATTACKED)
		s__DDDamageDetect___NCasts[ArrayId(GetAttacker())]=0
		u=GetTriggerUnit()
		id=ArrayId(u)
		if(not(HaveSavedInteger(s__groupex_HTABLE,2*((DDDamageDetect___Grp)-1),GetHandleId((u)))))
			s__groupex_AddUnit(DDDamageDetect___Grp,u)
		
		s__DDDamageDetect___NAttacks[id]=s__DDDamageDetect___NAttacks[id]+1
		DDDamageDetect___FullAttacks=DDDamageDetect___FullAttacks+1
		if(DDDamageDetect___FullAttacks<=010)
			// TriggerSleepAction(5.)
		
		DDDamageDetect___FullAttacks=DDDamageDetect___FullAttacks-1
		if(DDDamageDetect___FullAttacks==0)
			s__groupex_For(DDDamageDetect___Grp,function DDDamageDetect___EnumUnitsFree)
		
		u=null
	else if(GetTriggerEventId()==DDDamageDetect___EVENT_ENTER_REGION)
		TriggerRegisterUnitEvent(DDDamageDetect___Trig,GetTriggerUnit(),EVENT_UNIT_DAMAGED)
	else
		h__DestroyTrigger(DDDamageDetect___Trig)
		DDDamageDetect___Trig=CreateTrigger()
		TriggerAddCondition((DDDamageDetect___Trig),Condition((function DDDamageDetect___ExecuteTriggers)))
		s__groupex_FilterRect(bj_mapInitialPlayableArea,function DDDamageDetect___RegisterAllUnits)
	

public function DDDamageDetect___Init ()
	trigger t=CreateTrigger()
	integer i=0
	DDDamageDetect___Grp=(s__groupex_create())
	DDDamageDetect___Trig=CreateTrigger()
	DDDamageDetect___Reg=CreateRegion()
	RegionAddRect(DDDamageDetect___Reg,bj_mapInitialPlayableArea)
	s__groupex_FilterRect(bj_mapInitialPlayableArea,function DDDamageDetect___RegisterAllUnits)
	TriggerAddCondition((DDDamageDetect___Trig),Condition((function DDDamageDetect___ExecuteTriggers)))
	i=0-1
	while true
		i=i+1
		if(i>=bj_MAX_PLAYER_SLOTS)
			break
		TriggerRegisterPlayerUnitEvent(t,Player(i),EVENT_PLAYER_UNIT_ATTACKED,(DD__this__ff))
		TriggerRegisterPlayerUnitEvent(t,Player(i),EVENT_PLAYER_UNIT_SPELL_CAST,(DD__this__ff))
	
	TriggerRegisterEnterRegion(t,DDDamageDetect___Reg,(DD__this__ff))
	TriggerRegisterTimerEvent(t,60.,true)
	TriggerAddAction(t,function DDDamageDetect___Actions)
	DDDamageDetect___MainTrig=t

