public function SlashStrike__DamageEnumUnit ()
	unit u=GetEnumUnit()
	unit dummy
	real x
	real y
	real cx
	real cy
	integer id
	real dist
	real rad
	real ops
	real adj
	real life
	integer size=64
	integer arrayx=QMod(GetHandleId(u),size)
	id=SlashStrike__TempId
	if(s__SlashStrike__UnitDamaged[SlashStrike__Array2D(arrayx,id,size-1)])
		u=null
		return
	
	x=GetUnitX(u)
	y=GetUnitY(u)
	cx=GetUnitX(s__SlashStrike__Caster[id])
	cy=GetUnitY(s__SlashStrike__Caster[id])
	dist=SquareRoot(SlashStrike__Pow2(cx-x)+SlashStrike__Pow2(cy-y))
	rad=(s__SlashStrike__CasterRadians[id]-SlashStrike__PI_HALF)-Atan2(y-cy,x-cx)
	ops=dist*Sin(rad)
	adj=ops/ Tan(rad)
	if((ops<=SlashStrike__SS_BLADE_HEIGHT*0.5)and(adj<=SlashStrike__SS_BLADE_WIDTH))
		s__SlashStrike__UnitDamaged[SlashStrike__Array2D(arrayx,id,size-1)]=true
		life=GetUnitState(u,UNIT_STATE_LIFE)
		UnitDamageTarget(s__SlashStrike__Caster[id],u,s__SlashStrike__Damage[s__SlashStrike__SS_SpellLevel[id]],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_UNIVERSAL,WEAPON_TYPE_METAL_HEAVY_SLICE)
		DestroyEffect(AddSpecialEffectTarget(SlashStrike__BLOOD_EFFECT,u,SlashStrike__ATTACH_POINT_ORIGIN))
		if(GetUnitState(u,UNIT_STATE_LIFE)==life)
			dummy=CreateUnit(GetOwningPlayer(s__SlashStrike__Caster[id]),SlashStrike__DUMMY_CODE,x,y,bj_UNIT_FACING)
			UnitDamageTarget(dummy,u,s__SlashStrike__Damage[s__SlashStrike__SS_SpellLevel[id]],true,false,ATTACK_TYPE_HERO,DAMAGE_TYPE_UNIVERSAL,WEAPON_TYPE_METAL_HEAVY_SLICE)
			RemoveUnit(dummy)
			dummy=null
		
		if(SlashStrike__USE_PHYSICS_ENGINE)
			if(not IsUnitType(u,UNIT_TYPE_DEAD))
				u=null
				return
			
			GroupAddUnit(s__SlashStrike__BlowedUnitsGroup[id],u)
			SetUnitPathing(u,false)
			if(s__SlashStrike__CircleMove[id])
				s__SlashStrike__PhysicsBlowAngle[SlashStrike__Array2D(arrayx,id,size-1)]=s__SlashStrike__CasterRadians[id]-(adj*(SlashStrike__PI_HALF/ SlashStrike__SS_BLADE_WIDTH))
			else
				s__SlashStrike__PhysicsBlowAngle[SlashStrike__Array2D(arrayx,id,size-1)]=s__SlashStrike__CasterRadians[id]
			
			s__SlashStrike__PhysicsUnitForce[SlashStrike__Array2D(arrayx,id,size-1)]=GetRandomReal(200.,290.)
			s__SlashStrike__PhysicsUnitVelocity[SlashStrike__Array2D(arrayx,id,size-1)]=GetRandomReal(450.,600.)
			s__SlashStrike__PhysicsFloorZ[SlashStrike__Array2D(arrayx,id,size-1)]=GetUnitFlyHeight(u)
			UnitAddAbility(u,SlashStrike__CROW_FORM_RAWCODE)
			UnitRemoveAbility(u,SlashStrike__CROW_FORM_RAWCODE)
		
	
	u=null

