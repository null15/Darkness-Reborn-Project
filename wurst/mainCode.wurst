public function DarkTorture___SetSoundVolumeByDistance (sound s,real max_volume,real x,real y )
	integer i=0
	real cx
	real cy
	real d
	while true
		if(i>=bj_MAX_PLAYERS)
			break
		if(GetLocalPlayer()==s__DD_p[i])
			cx=GetCameraTargetPositionX()
			cy=GetCameraTargetPositionY()
			d=SquareRoot((cx-x)*(cx-x)+(cy-y)*(cy-y))
