		if i==bj_MAX_PLAYER_SLOTS
			break
	
	TriggerAddCondition(gg_trg_Crush,Condition(function Crush___Crush_Conditions))
	TriggerAddAction(gg_trg_Crush,function Crush___Crush_Actions)
	upre=CreateUnit(Player(14),Crush___DummyId,PreX,PreY,0)
	UnitAddAbility(upre,Crush___BuffAddId)
	RemoveUnit(upre)
	upre=null
	Preload(Crush___PushMdl)

