public function DarkTorture___DarkTorture_Main ()
	trigger t=CreateTrigger()
	integer i=0
	DarkTorture___DarkTorture_Setup()
	while true
		if(i>=DarkTorture___Levels)
			break
		if(DarkTorture___MaxSwordCount[i]>16)
			h__DestroyTrigger(t)
			return
		
		i=i+1
	
	TriggerRegisterAnyUnitEvent(t,EVENT_PLAYER_UNIT_SPELL_EFFECT)
	TriggerAddCondition(t,Condition(function DarkTorture___DarkTorture_Condition))
	TriggerAddAction(t,function DarkTorture___DarkTorture_Action)
