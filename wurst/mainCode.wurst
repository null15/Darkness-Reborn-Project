public function s__DarkTorture___sword_MoveEffects (integer this_ )
	real vel
	real rad
	real x
	real y
	if(s__DarkTorture___sword_EffectDist[this_]==0.)
		return
	
	vel=DarkTorture___TIMER_MIN_DELAY*DarkTorture___SWORDS_SPEED
	rad=GetUnitFacing(s__DarkTorture___sword_DummyEffect1[this_])*bj_DEGTORAD
	x=Cos(rad)
	y=Sin(rad)
	if(DarkTorture___EFFECTS_MAX_DISTANCE>0.)
		s__DarkTorture___sword_EffectDist[this_]=s__DarkTorture___sword_EffectDist[this_]+vel
	else
		s__DarkTorture___sword_EffectDist[this_]=s__DarkTorture___sword_EffectDist[this_]-vel
		vel=-vel
	
	if(s__DarkTorture___sword_EffectDist[this_]*s__DarkTorture___sword_EffectDist[this_]>=DarkTorture___EFFECTS_MAX_DISTANCE*DarkTorture___EFFECTS_MAX_DISTANCE)
		s__DarkTorture___sword_EffectDist[this_]=0.
	
	SetUnitX(s__DarkTorture___sword_DummyEffect1[this_],GetUnitX(s__DarkTorture___sword_DummyEffect1[this_])+vel*x)
	SetUnitY(s__DarkTorture___sword_DummyEffect1[this_],GetUnitY(s__DarkTorture___sword_DummyEffect1[this_])+vel*y)
	SetUnitX(s__DarkTorture___sword_DummyEffect2[this_],GetUnitX(s__DarkTorture___sword_DummyEffect2[this_])+vel*x)
	SetUnitY(s__DarkTorture___sword_DummyEffect2[this_],GetUnitY(s__DarkTorture___sword_DummyEffect2[this_])+vel*y)

