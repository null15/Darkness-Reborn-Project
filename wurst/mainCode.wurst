public function s__ParalyzingField__ParalyzingField_motion (integer this_ )
	integer p
	group g=GetUnitsInRange(ParalyzingField__PF_Range(s__ParalyzingField__ParalyzingField_lvl[this_]),s__ParalyzingField__ParalyzingField_x[this_],s__ParalyzingField__ParalyzingField_y[this_])
	unit a
	real def
	GroupRemoveUnit(g,s__ParalyzingField__ParalyzingField_u[this_])
	while true
		a=FirstOfGroup(g)
		if a==null
			break
		GroupRemoveUnit(g,a)
		if ParalyzingField__UnitFilter(s__ParalyzingField__ParalyzingField_u[this_],a)and not IsUnitInGroup(a,ParalyzingField__frozenall)
			UnitAddAbility(a,ParalyzingField__EffectId)
			def=GetUnitDefaultMoveSpeed(a)
			p=s__ParalyzingField__Paralyzed__allocate()
			s__ParalyzingField__Paralyzed_motiontype[p]=1
			s__ParalyzingField__Paralyzed_u[p]=a
			s__ParalyzingField__Paralyzed_fade[p]=def
			s__ParalyzingField__Paralyzed_default[p]=def
			s__ParalyzingField__Paralyzed_time[p]=0.
			s__ParalyzingField__Paralyzed_speed[p]=def/ ParalyzingField__InitFreeze*ParalyzingField__Checkperiodic
			s__ParalyzingField__Paralyzed_motiontype[p]=1
			s__ParalyzingField__Paralyzed_distance[p]=ParalyzingField__PF_Range(s__ParalyzingField__ParalyzingField_lvl[this_])
			s__ParalyzingField__Paralyzed_x[p]=s__ParalyzingField__ParalyzingField_x[this_]
			s__ParalyzingField__Paralyzed_y[p]=s__ParalyzingField__ParalyzingField_y[this_]
			s__ParalyzingField__Paralyzed_g[p]=s__ParalyzingField__ParalyzingField_frozen[this_]
			s__ParalyzingField__Paralyzed_addMotion(p)
			GroupAddUnit(s__ParalyzingField__ParalyzingField_frozen[this_],a)
			GroupAddUnit(ParalyzingField__frozenall,a)
			AddHandleIndex(a)
			s__ParalyzingField__TargetBuffData[(LoadInteger(HIS__tempcache,GetHandleId((a)),HIS__pos))]=p
		
	
	g=null

public function s__ParalyzingField__ParalyzingField_endmotion (integer this_ )
	integer p
	unit a
	while true
		a=FirstOfGroup(s__ParalyzingField__ParalyzingField_frozen[this_])
		if a==null
			break
		GroupRemoveUnit(s__ParalyzingField__ParalyzingField_frozen[this_],a)
		GroupRemoveUnit(ParalyzingField__frozenall,a)
		p=s__ParalyzingField__TargetBuffData[(LoadInteger(HIS__tempcache,GetHandleId((a)),HIS__pos))]
		s__ParalyzingField__Paralyzed_active[p]=false
	
	ClearHandleIndex(s__ParalyzingField__ParalyzingField_u[this_])
	GroupClear(s__ParalyzingField__ParalyzingField_frozen[this_])
	RecycleGroup(s__ParalyzingField__ParalyzingField_frozen[this_])
	DestroyEffect(s__ParalyzingField__ParalyzingField_mdl[this_])
	RemoveUnit(s__ParalyzingField__ParalyzingField_dummy[this_])
	s__ParalyzingField__ParalyzingField_u[this_]=null
	s__ParalyzingField__ParalyzingField_dummy[this_]=null
	s__ParalyzingField__ParalyzingField_frozen[this_]=null
	s__ParalyzingField__ParalyzingField_mdl[this_]=null
	s__ParalyzingField__ParalyzingField_deallocate(this_)

public function s__ParalyzingField__ParalyzingField_loopRun ()
	integer index=0
	while true
		if index==s__ParalyzingField__ParalyzingField_size
			break
		if not s__ParalyzingField__ParalyzingField_paused[s__s__ParalyzingField__ParalyzingField_runStruct[index]]
			if s__ParalyzingField__ParalyzingField_active[s__s__ParalyzingField__ParalyzingField_runStruct[index]]
				s__ParalyzingField__ParalyzingField_motion(s__s__ParalyzingField__ParalyzingField_runStruct[index])
				index=index+1
			else
				s__ParalyzingField__ParalyzingField_endmotion(s__s__ParalyzingField__ParalyzingField_runStruct[index])
				s__ParalyzingField__ParalyzingField_size=s__ParalyzingField__ParalyzingField_size-1
				s__s__ParalyzingField__ParalyzingField_runStruct[index]=s__s__ParalyzingField__ParalyzingField_runStruct[s__ParalyzingField__ParalyzingField_size]
			
		else
			index=index+1
		
	
	if s__ParalyzingField__ParalyzingField_size==0 
		PauseTimer(s__ParalyzingField__ParalyzingField_loopExe)
	

