	if(not(IsTriggerEnabled(gg_trg_Q_Mysterious_Magic_Sum)==false))
		return false
	
	return true

public function Trig_Q_Mysterious_Magic_Actions ()
	udg_S_MysteriousMagic_U[0]=GetSpellAbilityUnit()
	udg_S_MysteriousMagic_P[0]=GetUnitLoc(udg_S_MysteriousMagic_U[0])
	udg_S_MysteriousMagic_P[1]=GetSpellTargetLoc()
	udg_S_MysteriousMagic_Level[1]=I2R(GetUnitAbilityLevelSwapped('A0ZT',udg_S_MysteriousMagic_U[0]))
	udg_S_MysteriousMagic_D[0]=((15.00+(15.00*udg_S_MysteriousMagic_Level[1]))+((0.30+(0.10*udg_S_MysteriousMagic_Level[1]))*I2R(GetHeroStatBJ(bj_HEROSTAT_STR,udg_S_MysteriousMagic_U[0],true))))
	CreateNUnitsAtLocFacingLocBJ(1,'h048',GetOwningPlayer(udg_S_MysteriousMagic_U[0]),udg_S_MysteriousMagic_P[0],udg_S_MysteriousMagic_P[1])
	udg_S_MysteriousMagic_FaceAngle[0]=GetUnitFacing(GetLastCreatedUnit())
	RemoveUnit(GetLastCreatedUnit())
	if(Trig_Q_Mysterious_Magic_Func013C())
		EnableTrigger(gg_trg_Q_Mysterious_Magic_Sum)
	else
	
	RemoveLocation(udg_S_MysteriousMagic_P[0])
	RemoveLocation(udg_S_MysteriousMagic_P[1])

