public function DDDamageDetect___ExecuteTriggers () returns boolean
	integer i=0
	integer j=-1
	if(GetEventDamage()==0.)
		return(false)
	
	i=0-1
	while true
		i=i+1
		if(i>=((DDDamageDetect___Count-1)/ 4)+1)
			break
		j=j+1
		if(IsTriggerEnabled(s__DDDamageDetect___RegTrig[j]))
			if(TriggerEvaluate(s__DDDamageDetect___RegTrig[j]))
				TriggerExecute(s__DDDamageDetect___RegTrig[j])
			
		
		j=j+1
		if(IsTriggerEnabled(s__DDDamageDetect___RegTrig[j]))
			if(TriggerEvaluate(s__DDDamageDetect___RegTrig[j]))
				TriggerExecute(s__DDDamageDetect___RegTrig[j])
			
		
		j=j+1
		if(IsTriggerEnabled(s__DDDamageDetect___RegTrig[j]))
			if(TriggerEvaluate(s__DDDamageDetect___RegTrig[j]))
				TriggerExecute(s__DDDamageDetect___RegTrig[j])
			
		
		j=j+1
		if(IsTriggerEnabled(s__DDDamageDetect___RegTrig[j]))
			if(TriggerEvaluate(s__DDDamageDetect___RegTrig[j]))
				TriggerExecute(s__DDDamageDetect___RegTrig[j])
			
		
	
	j=ArrayId(GetTriggerUnit())
	if(s__DDDamageDetect___NAttacks[j]>0)
		s__DDDamageDetect___NAttacks[j]=s__DDDamageDetect___NAttacks[j]-1
	
	return(false)

